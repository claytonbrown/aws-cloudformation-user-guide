{
  "kinesis-streamconsumer-properties": {
    "ConsumerName": {
      "Required": "Yes",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "128",
      "Pattern": "[a-zA-Z0-9_.-]+",
      "UpdateRequires": "Replacement"
    },
    "StreamARN": {
      "Required": "Yes",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "2048",
      "Pattern": "arn:aws.*:kinesis:.*:\\d{12}:stream/.+",
      "UpdateRequires": "Replacement"
    }
  }
}
