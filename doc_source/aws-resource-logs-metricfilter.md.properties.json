{
  "logs-metricfilter-properties": {
    "MetricTransformations": {
      "Required": "Yes",
      "Type": "List of [MetricTransformation",
      "Maximum": "1",
      "UpdateRequires": "No interruption"
    },
    "FilterPattern": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "No interruption"
    },
    "LogGroupName": {
      "Pattern": "[\\.\\-_/#A-Za-z0-9]+",
      "Required": "Yes",
      "Maximum": "512",
      "UpdateRequires": "Replacement",
      "Minimum": "1",
      "Type": "String"
    }
  }
}
