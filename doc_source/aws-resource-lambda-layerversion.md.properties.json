{
  "lambda-layerversion-properties": {
    "Content": {
      "Required": "Yes",
      "Type": "Content",
      "UpdateRequires": "Replacement"
    },
    "LayerName": {
      "Pattern": "(arn:[a-zA-Z0-9-]+:lambda:[a-zA-Z0-9-]+:\\d{12}:layer:[a-zA-Z0-9-_]+)|[a-zA-Z0-9-_]+",
      "Required": "No",
      "Maximum": "140",
      "UpdateRequires": "Replacement",
      "Minimum": "1",
      "Type": "String"
    },
    "Description": {
      "Minimum": "0",
      "Required": "No",
      "Type": "String",
      "Maximum": "256",
      "UpdateRequires": "Replacement"
    },
    "LicenseInfo": {
      "Required": "No",
      "Type": "String",
      "Maximum": "512",
      "UpdateRequires": "Replacement"
    },
    "CompatibleRuntimes": {
      "Required": "No",
      "Type": "List of String",
      "Maximum": "5",
      "UpdateRequires": "Replacement"
    }
  }
}
