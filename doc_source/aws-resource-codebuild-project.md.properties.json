{
  "codebuild-project-properties": {
    "Artifacts": {
      "Required": "Yes",
      "Type": "Artifacts",
      "UpdateRequires": "No interruption"
    },
    "BadgeEnabled": {
      "AllowedValues": [
        "True",
        "False"
      ]
    },
    "Cache": {
      "Required": "No",
      "Type": "ProjectCache",
      "UpdateRequires": "No interruption"
    },
    "Description": {
      "Required": "No",
      "Type": "String",
      "Minimum": "0",
      "Maximum": "255",
      "UpdateRequires": "No interruption"
    },
    "EncryptionKey": {
      "Required": "No",
      "Type": "String",
      "Minimum": "1",
      "UpdateRequires": "No interruption"
    },
    "Environment": {
      "Required": "Yes",
      "Type": "Environment",
      "UpdateRequires": "No interruption"
    },
    "LogsConfig": {
      "Required": "No",
      "Type": "LogsConfig",
      "UpdateRequires": "No interruption"
    },
    "Name": {
      "Required": "No",
      "Type": "String",
      "Minimum": "2",
      "Maximum": "255",
      "Pattern": "[A-Za-z0-9][A-Za-z0-9\\-_]{1,254}",
      "UpdateRequires": "Replacement"
    },
    "QueuedTimeoutInMinutes": {
      "Required": "No",
      "Type": "Integer",
      "Minimum": "5",
      "Maximum": "480",
      "UpdateRequires": "No interruption"
    },
    "SecondaryArtifacts": {
      "Required": "No",
      "Type": "List of [Artifacts",
      "Maximum": "12",
      "UpdateRequires": "No interruption"
    },
    "SecondarySourceVersions": {
      "Required": "No",
      "Type": "List of [ProjectSourceVersion",
      "Maximum": "12",
      "UpdateRequires": "No interruption"
    },
    "SecondarySources": {
      "Required": "No",
      "Type": "List of [Source",
      "Maximum": "12",
      "UpdateRequires": "No interruption"
    },
    "ServiceRole": {
      "Required": "Yes",
      "Type": "String",
      "Minimum": "1",
      "UpdateRequires": "No interruption"
    },
    "Source": {
      "Required": "Yes",
      "Type": "Source",
      "UpdateRequires": "No interruption"
    },
    "SourceVersion": {},
    "Tags": {
      "Required": "No",
      "Type": "List of [Tag",
      "Maximum": "50",
      "UpdateRequires": "No interruption"
    },
    "TimeoutInMinutes": {
      "Required": "No",
      "Type": "Integer",
      "Minimum": "5",
      "Maximum": "480",
      "UpdateRequires": "No interruption"
    },
    "Triggers": {
      "Required": "No",
      "Type": "ProjectTriggers",
      "UpdateRequires": "No interruption"
    },
    "VpcConfig": {}
  }
}
