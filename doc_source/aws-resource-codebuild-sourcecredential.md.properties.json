{
  "codebuild-sourcecredential-properties": {
    "AuthType": {
      "Required": "Yes",
      "Type": "String",
      "AllowedValues": [
        "BASIC_AUTH",
        "OAUTH",
        "PERSONAL_ACCESS_TOKEN"
      ],
      "UpdateRequires": "No interruption"
    },
    "ServerType": {
      "Required": "Yes",
      "Type": "String",
      "AllowedValues": [
        "BITBUCKET",
        "GITHUB",
        "GITHUB_ENTERPRISE"
      ],
      "UpdateRequires": "Replacement"
    },
    "Token": {
      "Required": "Yes",
      "Type": "String",
      "Minimum": "1",
      "UpdateRequires": "No interruption"
    },
    "Username": {
      "Required": "No",
      "Type": "String",
      "Minimum": "1",
      "UpdateRequires": "No interruption"
    }
  }
}
