{
  "aws-properties-ecs-taskdefinition-containerdependency-properties": {
    "ContainerName": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "Condition": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "Replacement",
      "AllowedValues": [
        "COMPLETE",
        "HEALTHY",
        "START",
        "SUCCESS"
      ]
    }
  }
}
