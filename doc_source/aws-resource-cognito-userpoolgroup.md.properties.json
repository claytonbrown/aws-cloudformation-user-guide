{
  "cognito-userpoolgroup-properties": {
    "UserPoolId": {
      "Pattern": "[\\w-]+_[0-9a-zA-Z]+",
      "Required": "Yes",
      "Maximum": "55",
      "UpdateRequires": "Replacement",
      "Minimum": "1",
      "Type": "String"
    },
    "GroupName": {
      "Pattern": "[\\p{L}\\p{M}\\p{S}\\p{N}\\p{P}]+",
      "Required": "No",
      "Maximum": "128",
      "UpdateRequires": "Replacement",
      "Minimum": "1",
      "Type": "String"
    },
    "RoleArn": {
      "Pattern": "arn:[\\w+=/,.@-]+:[\\w+=/,.@-]+:([\\w+=/,.@-]*)?:[0-9]+:[\\w+=/,.@-]+(:[\\w+=/,.@-]+)?(:[\\w+=/,.@-]+)?",
      "Required": "No",
      "Maximum": "2048",
      "UpdateRequires": "No interruption",
      "Minimum": "20",
      "Type": "String"
    },
    "Description": {
      "Required": "No",
      "Type": "String",
      "Maximum": "2048",
      "UpdateRequires": "No interruption"
    },
    "Precedence": {
      "Minimum": "0",
      "Required": "No",
      "Type": "Double",
      "UpdateRequires": "No interruption"
    }
  }
}
