        "GOOGLE_DRIVE",
        "HOMEFOLDERS",
        "ONE_DRIVE"
      ],
      "UpdateRequires": "No interruption"
    },
    "Domains": {
      "Required": "No",
      "Type": "List of String",
      "Maximum": "10",
      "UpdateRequires": "No interruption"
    },
    "ResourceIdentifier": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "No interruption"
    }
  }
}
