{
  "logs-destination-properties": {
    "DestinationName": {
      "Required": "Yes",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "512",
      "Pattern": "[^:*]*",
      "UpdateRequires": "Replacement"
    },
    "DestinationPolicy": {
      "Required": "Yes",
      "Type": "String",
      "Minimum": "1",
      "UpdateRequires": "No interruption"
    },
    "RoleArn": {
      "Required": "Yes",
      "Type": "String",
      "Minimum": "1",
      "UpdateRequires": "No interruption"
    },
    "TargetArn": {
      "Required": "Yes",
      "Type": "String",
      "Minimum": "1",
      "UpdateRequires": "No interruption"
    }
  }
}
