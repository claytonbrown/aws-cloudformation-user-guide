{
  "ivs.channel.RecordingConfigurationArn": {
    "Default": "\"\" \\(empty string, recording is disabled\\)",
    "Pattern": "^$|^arn:aws:ivs:[a-z0-9-]+:[0-9]+:recording-configuration/[a-zA-Z0-9-]+$",
    "Required": "No",
    "Maximum": "128",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)",
    "Minimum": "0",
    "Type": "String"
  },
  "ivs.channel.LatencyMode": {
    "Default": "LOW",
    "Allowed values": "LOW | NORMAL",
    "Required": "No",
    "Type": "String",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "ivs.channel.Type": {
    "Required": "No",
    "UpdateRequires": "No interruption"
  },
  "ivs.channel.Tags": {
    "Required": "No",
    "Type": "List of [Tag",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "ivs.channel.Name": {
    "Pattern": "^[a-zA-Z0-9-_]*$",
    "Required": "No",
    "Maximum": "128",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)",
    "Minimum": "0",
    "Type": "String"
  },
  "ivs.channel.Authorized": {
    "Default": "false",
    "Required": "No",
    "Type": "Boolean",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)",
    "AllowedValues": [
      "True",
      "False"
    ]
  }
}
