{
  "aws-properties-applicationautoscaling-scalingpolicy-stepscalingpolicyconfiguration-properties": {
    "MinAdjustmentMagnitude": {
      "Required": "No",
      "Type": "Integer",
      "UpdateRequires": "No interruption"
    },
    "MetricAggregationType": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "Average",
        "Maximum",
        "Minimum"
      ]
    },
    "Cooldown": {
      "Required": "No",
      "Type": "Integer",
      "UpdateRequires": "No interruption"
    },
    "StepAdjustments": {
      "Required": "No",
      "Type": "List of [StepAdjustment",
      "UpdateRequires": "No interruption"
    },
    "AdjustmentType": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "ChangeInCapacity",
        "ExactCapacity",
        "PercentChangeInCapacity"
      ]
    }
  }
}
