{
  "aws-properties-ssm-patchbaseline-rule-properties": {
    "EnableNonSecurity": {
      "Required": "No",
      "Type": "Boolean",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "True",
        "False"
      ]
    },
    "ComplianceLevel": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "CRITICAL",
        "HIGH",
        "INFORMATIONAL",
        "LOW",
        "MEDIUM",
        "UNSPECIFIED"
      ]
    },
    "ApproveAfterDays": {
      "Minimum": "0",
      "Required": "No",
      "Type": "Integer",
      "Maximum": "100",
      "UpdateRequires": "No interruption"
    },
    "PatchFilterGroup": {
      "Required": "No",
      "Type": "PatchFilterGroup",
      "UpdateRequires": "No interruption"
    }
  }
}
