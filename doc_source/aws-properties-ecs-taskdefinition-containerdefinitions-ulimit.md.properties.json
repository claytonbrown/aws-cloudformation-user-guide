{
  "aws-properties-ecs-taskdefinition-containerdefinitions-ulimit-properties": {
    "SoftLimit": {
      "Required": "Yes",
      "Type": "Integer",
      "UpdateRequires": "Replacement"
    },
    "HardLimit": {
      "Required": "Yes",
      "Type": "Integer",
      "UpdateRequires": "Replacement"
    },
    "Name": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "Replacement",
      "AllowedValues": [
        "core",
        "cpu",
        "data",
        "fsize",
        "locks",
        "memlock",
        "msgqueue",
        "nice",
        "nofile",
        "nproc",
        "rss",
        "rtprio",
        "rttime",
        "sigpending",
        "stack"
      ]
    }
  }
}
