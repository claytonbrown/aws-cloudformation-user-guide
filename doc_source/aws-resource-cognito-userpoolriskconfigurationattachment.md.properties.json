{
  "cognito-userpoolriskconfigurationattachment-properties": {
    "RiskExceptionConfiguration": {
      "Required": "No",
      "Type": "RiskExceptionConfigurationType",
      "UpdateRequires": "No interruption"
    },
    "CompromisedCredentialsRiskConfiguration": {
      "Required": "No",
      "Type": "CompromisedCredentialsRiskConfigurationType",
      "UpdateRequires": "No interruption"
    },
    "AccountTakeoverRiskConfiguration": {
      "Required": "No",
      "Type": "AccountTakeoverRiskConfigurationType",
      "UpdateRequires": "No interruption"
    },
    "UserPoolId": {
      "Pattern": "[\\w-]+_[0-9a-zA-Z]+",
      "Required": "Yes",
      "Maximum": "55",
      "UpdateRequires": "Replacement",
      "Minimum": "1",
      "Type": "String"
    },
    "ClientId": {
      "Pattern": "[\\w+]+",
      "Required": "Yes",
      "Maximum": "128",
      "UpdateRequires": "Replacement",
      "Minimum": "1",
      "Type": "String"
    }
  }
}
