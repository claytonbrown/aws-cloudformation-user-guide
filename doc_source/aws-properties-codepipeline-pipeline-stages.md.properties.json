{
  "aws-properties-codepipeline-pipeline-stages-properties": {
    "Actions": {
      "Required": "Yes",
      "Type": "List of [ActionDeclaration",
      "UpdateRequires": "No interruption"
    },
    "Blockers": {
      "Required": "No",
      "Type": "List of [BlockerDeclaration",
      "UpdateRequires": "No interruption"
    },
    "Name": {
      "Required": "Yes",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "100",
      "Pattern": "[A-Za-z0-9.@\\-_]+",
      "UpdateRequires": "No interruption"
    }
  }
}
