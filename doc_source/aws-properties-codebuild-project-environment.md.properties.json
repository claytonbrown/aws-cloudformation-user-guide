{
  "aws-properties-codebuild-project-environment-properties": {
    "Certificate": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "No interruption"
    },
    "ComputeType": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "No interruption"
    },
    "EnvironmentVariables": {
      "Required": "No",
      "Type": "List of [EnvironmentVariable",
      "UpdateRequires": "No interruption"
    },
    "Image": {
      "Required": "Yes",
      "Type": "String",
      "Minimum": "1",
      "UpdateRequires": "No interruption"
    },
    "ImagePullCredentialsType": {
      "Required": "No",
      "Type": "String",
      "AllowedValues": [
        "CODEBUILD",
        "SERVICE_ROLE"
      ],
      "UpdateRequires": "No interruption"
    },
    "PrivilegedMode": {
      "AllowedValues": [
        "True",
        "False"
      ]
    },
    "RegistryCredential": {
      "Required": "No",
      "Type": "RegistryCredential",
      "UpdateRequires": "No interruption"
    },
    "Type": {
      "Required": "Yes",
      "Type": "String",
      "AllowedValues": [
        "LINUX_CONTAINER",
        "WINDOWS_CONTAINER"
      ],
      "UpdateRequires": "No interruption"
    }
  }
}
