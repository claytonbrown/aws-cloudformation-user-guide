{
  "appstream-stackuserassociation-properties": {
    "StackName": {
      "Minimum": "1",
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "UserName": {
      "Pattern": "[\\p{L}\\p{M}\\p{S}\\p{N}\\p{P}]+",
      "Required": "Yes",
      "Maximum": "128",
      "UpdateRequires": "Replacement",
      "Minimum": "1",
      "Type": "String"
    },
    "SendEmailNotification": {
      "Required": "No",
      "Type": "Boolean",
      "UpdateRequires": "Replacement",
      "AllowedValues": [
        "True",
        "False"
      ]
    },
    "AuthenticationType": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "Replacement"
    }
  }
}
