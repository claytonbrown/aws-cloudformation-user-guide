{
  "appstream-stackuserassociation-properties": {
    "AuthenticationType": {
      "Required": "Yes",
      "Type": "String",
      "AllowedValues": [
        "API",
        "SAML",
        "USERPOOL"
      ],
      "UpdateRequires": "Replacement"
    },
    "SendEmailNotification": {
      "Required": "No",
      "AllowedValues": [
        "True",
        "False"
      ],
      "Type": "Boolean",
      "UpdateRequires": "Replacement"
    },
    "StackName": {
      "Required": "Yes",
      "Type": "String",
      "Minimum": "1",
      "UpdateRequires": "Replacement"
    },
    "UserName": {
      "Required": "Yes",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "128",
      "Pattern": "[\\p{L}\\p{M}\\p{S}\\p{N}\\p{P}]+",
      "UpdateRequires": "Replacement"
    }
  }
}
