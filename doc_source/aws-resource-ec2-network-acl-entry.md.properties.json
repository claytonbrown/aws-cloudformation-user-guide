{
  "ec2-network-acl-entry-properties": {
    "NetworkAclId": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "RuleNumber": {
      "Required": "Yes",
      "Type": "Integer",
      "UpdateRequires": "Replacement"
    },
    "Protocol": {
      "Required": "Yes",
      "Type": "Integer",
      "UpdateRequires": "No interruption"
    },
    "Ipv6CidrBlock": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "No interruption"
    },
    "Egress": {
      "Required": "No",
      "Type": "Boolean",
      "UpdateRequires": "Replacement",
      "AllowedValues": [
        "True",
        "False"
      ]
    },
    "RuleAction": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "allow",
        "deny"
      ]
    },
    "PortRange": {
      "Required": "No",
      "Type": "PortRange",
      "UpdateRequires": "No interruption"
    },
    "Icmp": {
      "Required": "No",
      "Type": "Icmp",
      "UpdateRequires": "No interruption"
    },
    "CidrBlock": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "No interruption"
    }
  }
}
