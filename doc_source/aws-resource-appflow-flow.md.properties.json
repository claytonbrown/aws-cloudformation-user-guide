{
  "appflow.flow.SourceFlowConfig": {
    "Required": "Yes",
    "Type": "SourceFlowConfig",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "appflow.flow.KMSArn": {
    "Pattern": "arn:aws:kms:.*:[0-9]+:.*",
    "Required": "No",
    "Maximum": "2048",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)",
    "Minimum": "20",
    "Type": "String"
  },
  "appflow.flow.Tags": {
    "Required": "No",
    "Type": "List of [Tag",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "appflow.flow.TriggerConfig": {
    "Required": "Yes",
    "Type": "TriggerConfig",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)\n\n#"
  },
  "appflow.flow.Tasks": {
    "Required": "Yes",
    "Type": "List of [Task",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "appflow.flow.FlowName": {
    "Pattern": "[a-zA-Z0-9][\\w!@#.-]+",
    "Required": "Yes",
    "Type": "String",
    "Maximum": "256",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)"
  },
  "appflow.flow.Description": {
    "Pattern": "[\\w!@#\\-.?,\\s]*",
    "Required": "No",
    "Type": "String",
    "Maximum": "2048",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "appflow.flow.DestinationFlowConfigList": {
    "Required": "Yes",
    "Type": "List of [DestinationFlowConfig",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  }
}
