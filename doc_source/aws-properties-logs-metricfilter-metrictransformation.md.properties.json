      "UpdateRequires": "No interruption"
    },
    "MetricName": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "No interruption"
    },
    "MetricNamespace": {
      "Required": "Yes",
      "Type": "String",
      "Maximum": "255",
      "Pattern": "[^:*$]*",
      "UpdateRequires": "No interruption"
    },
    "MetricValue": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "No interruption"
    }
  }
}
