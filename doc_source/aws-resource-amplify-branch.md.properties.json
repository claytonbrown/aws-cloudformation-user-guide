{
  "amplify-branch-properties": {
    "AppId": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "BasicAuthConfig": {
      "Required": "No",
      "Type": "BasicAuthConfig",
      "UpdateRequires": "No interruption"
    },
    "BranchName": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "BuildSpec": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "No interruption"
    },
    "Description": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "No interruption"
    },
    "EnableAutoBuild": {
      "Required": "No",
      "AllowedValues": [
        "True",
        "False"
      ],
      "Type": "Boolean",
      "UpdateRequires": "No interruption"
    },
    "EnvironmentVariables": {
      "Required": "No",
      "Type": "List of [EnvironmentVariable",
      "UpdateRequires": "No interruption"
    },
    "Stage": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "No interruption"
    },
    "Tags": {
      "Required": "No",
      "Type": "List of [Tag",
      "UpdateRequires": "No interruption"
    }
  }
}
