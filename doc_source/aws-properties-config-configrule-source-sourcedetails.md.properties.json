        "aws.config"
      ],
      "UpdateRequires": "No interruption"
    },
    "MaximumExecutionFrequency": {
      "Required": "No",
      "Type": "String",
      "AllowedValues": [
        "One_Hour",
        "Six_Hours",
        "Three_Hours",
        "Twelve_Hours",
        "TwentyFour_Hours"
      ],
      "UpdateRequires": "No interruption"
    },
    "MessageType": {
      "Required": "Yes",
      "Type": "String",
      "AllowedValues": [
        "ConfigurationItemChangeNotification",
        "ConfigurationSnapshotDeliveryCompleted",
        "OversizedConfigurationItemChangeNotification",
        "ScheduledNotification"
      ],
      "UpdateRequires": "No interruption"
    }
  }
}
