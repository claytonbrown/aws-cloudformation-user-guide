{
  "ssmcontacts.contactchannel.ChannelType": {
    "Allowed values": "EMAIL | SMS | VOICE",
    "Required": "Yes",
    "Type": "String",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)"
  },
  "ssmcontacts.contactchannel.ChannelName": {
    "Pattern": "^[a-zA-Z0-9_\\-\\s\\.]*$",
    "Required": "Yes",
    "Maximum": "255",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)",
    "Minimum": "1",
    "Type": "String"
  },
  "ssmcontacts.contactchannel.DeferActivation": {
    "Required": "No",
    "Type": "Boolean",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)\n\n#",
    "AllowedValues": [
      "True",
      "False"
    ]
  },
  "ssmcontacts.contactchannel.ContactId": {
    "Pattern": "arn:(aws|aws-cn|aws-us-gov):ssm-contacts:[-\\w+=\\/,.@]*:[0-9]+:([\\w+=\\/,.@:-]+)*",
    "Required": "Yes",
    "Maximum": "2048",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)",
    "Minimum": "1",
    "Type": "String"
  },
  "ssmcontacts.contactchannel.ChannelAddress": {
    "Required": "Yes",
    "Type": "String",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  }
}
