{
  "ssm-maintenancewindow-properties": {
    "Cutoff": {
      "Minimum": "0",
      "Required": "Yes",
      "Type": "Integer",
      "Maximum": "23",
      "UpdateRequires": "No interruption"
    },
    "EndDate": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "No interruption"
    },
    "Description": {
      "Minimum": "1",
      "Required": "No",
      "Type": "String",
      "Maximum": "128",
      "UpdateRequires": "No interruption"
    },
    "Schedule": {
      "Minimum": "1",
      "Required": "Yes",
      "Type": "String",
      "Maximum": "256",
      "UpdateRequires": "No interruption"
    },
    "Tags": {
      "Required": "No",
      "Type": "List of [Tag",
      "Maximum": "1000",
      "UpdateRequires": "No interruption"
    },
    "ScheduleTimezone": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "No interruption"
    },
    "AllowUnassociatedTargets": {
      "Required": "Yes",
      "Type": "Boolean",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "True",
        "False"
      ]
    },
    "Duration": {
      "Minimum": "1",
      "Required": "Yes",
      "Type": "Integer",
      "Maximum": "24",
      "UpdateRequires": "No interruption"
    },
    "StartDate": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "No interruption"
    },
    "Name": {
      "Pattern": "^[a-zA-Z0-9_\\-.]{3,128}$",
      "Required": "Yes",
      "Maximum": "128",
      "UpdateRequires": "No interruption",
      "Minimum": "3",
      "Type": "String"
    }
  }
}
