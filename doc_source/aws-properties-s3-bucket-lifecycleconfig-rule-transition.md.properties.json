{
  "aws-properties-s3-bucket-lifecycleconfig-rule-transition-properties": {
    "StorageClass": {
      "Required": "Yes",
      "Type": "String",
      "AllowedValues": [
        "DEEP_ARCHIVE",
        "GLACIER",
        "INTELLIGENT_TIERING",
        "ONEZONE_IA",
        "STANDARD_IA"
      ],
      "UpdateRequires": "No interruption"
    },
    "TransitionDate": {
      "Required": "Conditional",
      "Type": "Timestamp",
      "UpdateRequires": "No interruption"
    },
    "TransitionInDays": {
      "Required": "Conditional",
      "Type": "Integer",
      "UpdateRequires": "No interruption"
    }
  }
}
