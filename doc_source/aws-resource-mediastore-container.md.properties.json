{
  "mediastore-container-properties": {
    "Policy": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "No interruption"
    },
    "ContainerName": {
      "Pattern": "[\\w-]+",
      "Required": "Yes",
      "Maximum": "255",
      "UpdateRequires": "Replacement",
      "Minimum": "1",
      "Type": "String"
    },
    "CorsPolicy": {
      "Required": "No",
      "Type": "List of [CorsRule",
      "UpdateRequires": "No interruption"
    },
    "AccessLoggingEnabled": {
      "Required": "No",
      "Type": "Boolean",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "True",
        "False"
      ]
    },
    "LifecyclePolicy": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "No interruption"
    }
  }
}
