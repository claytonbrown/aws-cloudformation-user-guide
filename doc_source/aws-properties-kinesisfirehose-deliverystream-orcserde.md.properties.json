{
  "aws-properties-kinesisfirehose-deliverystream-orcserde-properties": {
    "Compression": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "NONE",
        "SNAPPY",
        "ZLIB"
      ]
    },
    "BloomFilterColumns": {
      "Required": "No",
      "Type": "List of String",
      "UpdateRequires": "No interruption"
    },
    "BlockSizeBytes": {
      "Minimum": "67108864",
      "Required": "No",
      "Type": "Integer",
      "UpdateRequires": "No interruption"
    },
    "PaddingTolerance": {
      "Required": "No",
      "Type": "Double",
      "UpdateRequires": "No interruption"
    },
    "StripeSizeBytes": {
      "Minimum": "8388608",
      "Required": "No",
      "Type": "Integer",
      "UpdateRequires": "No interruption"
    },
    "BloomFilterFalsePositiveProbability": {
      "Required": "No",
      "Type": "Double",
      "UpdateRequires": "No interruption"
    },
    "FormatVersion": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "V0_11",
        "V0_12"
      ]
    },
    "RowIndexStride": {
      "Minimum": "1000",
      "Required": "No",
      "Type": "Integer",
      "UpdateRequires": "No interruption"
    },
    "EnablePadding": {
      "Required": "No",
      "Type": "Boolean",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "True",
        "False"
      ]
    },
    "DictionaryKeyThreshold": {
      "Required": "No",
      "Type": "Double",
      "UpdateRequires": "No interruption"
    }
  }
}
