{
  "sagemaker-endpointconfig-properties": {
    "EndpointConfigName": {
      "Pattern": "^[a-zA-Z0-9](-*[a-zA-Z0-9])*",
      "Required": "No",
      "Type": "String",
      "Maximum": "63",
      "UpdateRequires": "Replacement"
    },
    "KmsKeyId": {
      "Pattern": ".*",
      "Required": "No",
      "Type": "String",
      "Maximum": "2048",
      "UpdateRequires": "Replacement"
    },
    "ProductionVariants": {
      "Required": "Yes",
      "Type": "List of [ProductionVariant",
      "UpdateRequires": "Replacement"
    },
    "Tags": {
      "Required": "No",
      "Type": "List of [Tag",
      "Maximum": "50",
      "UpdateRequires": "No interruption"
    }
  }
}
