      "Required": "Yes",
      "Maximum": "253",
      "UpdateRequires": "No interruption",
      "Minimum": "1",
      "Type": "String"
    },
    "DomainName": {
      "Pattern": "^(\\*\\.)?(((?!-)[A-Za-z0-9-]{0,62}[A-Za-z0-9])\\.)+((?!-)[A-Za-z0-9-]{1,62}[A-Za-z0-9])$",
      "Required": "Yes",
      "Maximum": "253",
      "UpdateRequires": "No interruption",
      "Minimum": "1",
      "Type": "String"
    }
  }
}
