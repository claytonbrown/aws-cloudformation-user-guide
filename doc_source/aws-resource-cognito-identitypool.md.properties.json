{
  "cognito-identitypool-properties": {
    "DeveloperProviderName": {
      "Type": "String",
      "Required": "No",
      "Maximum length": "100",
      "UpdateRequires": "No interruption",
      "Minimum length": "1"
    },
    "SamlProviderARNs": {
      "Required": "No",
      "Type": "List of String",
      "UpdateRequires": "No interruption"
    },
    "SupportedLoginProviders": {
      "Required": "No",
      "Type": "Json",
      "UpdateRequires": "No interruption"
    },
    "AllowUnauthenticatedIdentities": {
      "Required": "Yes",
      "Type": "Boolean",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "True",
        "False"
      ]
    },
    "CognitoIdentityProviders": {
      "Required": "No",
      "Type": "List of [CognitoIdentityProvider",
      "UpdateRequires": "No interruption"
    },
    "IdentityPoolName": {
      "Maximum length": "128",
      "Minimum length": "1",
      "Pattern": "[\\w ]+",
      "Required": "No",
      "UpdateRequires": "No interruption",
      "Type": "String"
    },
    "PushSync": {
      "Required": "No",
      "Type": "PushSync",
      "UpdateRequires": "No interruption"
    },
    "CognitoStreams": {
      "Required": "No",
      "Type": "CognitoStreams",
      "UpdateRequires": "No interruption"
    },
    "OpenIdConnectProviderARNs": {
      "Required": "No",
      "Type": "List of String",
      "UpdateRequires": "No interruption"
    },
    "CognitoEvents": {
      "Required": "No",
      "Type": "Json",
      "UpdateRequires": "No interruption"
    }
  }
}
