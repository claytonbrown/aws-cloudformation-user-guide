{
  "aws-properties-iotanalytics-dataset-s3destinationconfiguration-properties": {
    "GlueConfiguration": {
      "Required": "No",
      "Type": "GlueConfiguration",
      "UpdateRequires": "No interruption"
    },
    "Bucket": {
      "Pattern": "^[a-zA-Z0-9.\\-_]*$",
      "Required": "Yes",
      "Maximum": "255",
      "UpdateRequires": "No interruption",
      "Minimum": "3",
      "Type": "String"
    },
    "RoleArn": {
      "Minimum": "20",
      "Required": "Yes",
      "Type": "String",
      "Maximum": "2048",
      "UpdateRequires": "No interruption"
    },
    "Key": {
      "Pattern": "^[a-zA-Z0-9!_.*'()/{}:-]*$",
      "Required": "Yes",
      "Maximum": "255",
      "UpdateRequires": "No interruption",
      "Minimum": "1",
      "Type": "String"
    }
  }
}
