{
  "ec2-flowlog-properties": {
    "DeliverLogsPermissionArn": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "LogDestination": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "LogDestinationType": {
      "Required": "No",
      "Type": "String",
      "AllowedValues": [
        "cloud-watch-logs",
        "s3"
      ],
      "UpdateRequires": "Replacement"
    },
    "LogGroupName": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "ResourceId": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "ResourceType": {
      "Required": "Yes",
      "Type": "String",
      "AllowedValues": [
        "NetworkInterface",
        "Subnet",
        "VPC"
      ],
      "UpdateRequires": "Replacement"
    },
    "TrafficType": {
      "Required": "Yes",
      "Type": "String",
      "AllowedValues": [
        "ACCEPT",
        "ALL",
        "REJECT"
      ],
      "UpdateRequires": "Replacement"
    }
  }
}
