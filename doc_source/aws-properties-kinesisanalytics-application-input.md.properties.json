{
  "aws-properties-kinesisanalytics-application-input-properties": {
    "KinesisFirehoseInput": {
      "Required": "Conditional",
      "Type": "KinesisFirehoseInput",
      "UpdateRequires": "No interruption"
    },
    "KinesisStreamsInput": {
      "Required": "Conditional",
      "Type": "KinesisStreamsInput",
      "UpdateRequires": "No interruption"
    },
    "InputProcessingConfiguration": {
      "Required": "No",
      "Type": "InputProcessingConfiguration",
      "UpdateRequires": "No interruption"
    },
    "NamePrefix": {
      "Minimum": "1",
      "Required": "Yes",
      "Type": "String",
      "Maximum": "32",
      "UpdateRequires": "No interruption"
    },
    "InputParallelism": {
      "Required": "No",
      "Type": "InputParallelism",
      "UpdateRequires": "No interruption"
    },
    "InputSchema": {
      "Required": "Yes",
      "Type": "InputSchema",
      "UpdateRequires": "No interruption"
    }
  }
}
