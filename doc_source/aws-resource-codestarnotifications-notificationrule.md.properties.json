{
  "codestarnotifications.notificationrule.resource": {
    "Pattern": "^arn:aws[^:\\s]*:[^:\\s]*:[^:\\s]*:[0-9]{12}:[^\\s]+$",
    "Required": "Yes",
    "Type": "String",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)"
  },
  "codestarnotifications.notificationrule.tags": {
    "Required": "No",
    "Type": "Json",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)"
  },
  "codestarnotifications.notificationrule.name": {
    "Pattern": "[A-Za-z0-9\\-_ ]+$",
    "Required": "Yes",
    "Maximum": "64",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)",
    "Minimum": "1",
    "Type": "String"
  },
  "codestarnotifications.notificationrule.status": {
    "Allowed values": "DISABLED | ENABLED",
    "Required": "No",
    "Type": "String",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "codestarnotifications.notificationrule.detailtype": {
    "Allowed values": "BASIC | FULL",
    "Required": "Yes",
    "Type": "String",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "codestarnotifications.notificationrule.targets": {
    "Required": "Yes",
    "Type": "List of [Target",
    "Maximum": "10",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)\n\n#"
  },
  "codestarnotifications.notificationrule.eventtypeids": {
    "Required": "Yes",
    "UpdateRequires": "No interruption"
  }
}
