{
  "sagemaker.pipeline.RoleArn": {
    "Pattern": "^arn:aws[a-z\\-]*:iam::\\d{12}:role/?[a-zA-Z_0-9+=,.@\\-_/]+$",
    "Required": "Yes",
    "Maximum": "2048",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)",
    "Minimum": "20",
    "Type": "String"
  },
  "sagemaker.pipeline.PipelineDefinition": {
    "Required": "Yes",
    "Type": "Json",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "sagemaker.pipeline.PipelineName": {
    "Pattern": "^[a-zA-Z0-9](-*[a-zA-Z0-9]){0,255}",
    "Required": "Yes",
    "Maximum": "256",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)",
    "Minimum": "1",
    "Type": "String"
  },
  "sagemaker.pipeline.PipelineDisplayName": {
    "Pattern": "^[a-zA-Z0-9](-*[a-zA-Z0-9]){0,255}",
    "Required": "No",
    "Maximum": "256",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)",
    "Minimum": "1",
    "Type": "String"
  },
  "sagemaker.pipeline.Tags": {
    "Required": "No",
    "Type": "List of [Tag",
    "Maximum": "50",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)\n\n#"
  },
  "sagemaker.pipeline.PipelineDescription": {
    "Pattern": ".*",
    "Required": "No",
    "Maximum": "3072",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)",
    "Minimum": "0",
    "Type": "String"
  }
}
