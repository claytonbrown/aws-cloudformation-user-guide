{
  "aws-properties-managedblockchain-member-networkconfiguration-properties": {
    "Description": {
      "Required": "No",
      "Type": "String",
      "Maximum": "128",
      "UpdateRequires": "No interruption"
    },
    "Framework": {
      "Required": "Yes",
      "Type": "String",
      "AllowedValues": [
        "HYPERLEDGER_FABRIC"
      ],
      "UpdateRequires": "No interruption"
    },
    "FrameworkVersion": {
      "Required": "Yes",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "8",
      "UpdateRequires": "No interruption"
    },
    "Name": {
      "Required": "Yes",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "64",
      "Pattern": ".*\\S.*",
      "UpdateRequires": "No interruption"
    },
    "NetworkFrameworkConfiguration": {
      "Required": "No",
      "Type": "NetworkFrameworkConfiguration",
      "UpdateRequires": "No interruption"
    },
    "VotingPolicy": {
      "Required": "Yes",
      "Type": "VotingPolicy",
      "UpdateRequires": "No interruption"
    }
  }
}
