{
  "iotanalytics-datastore-properties": {
    "DatastoreName": {
      "Required": "No",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "128",
      "Pattern": "^[a-zA-Z0-9_]+$",
      "UpdateRequires": "Replacement"
    },
    "DatastoreStorage": {
      "Required": "No",
      "Type": "DatastoreStorage",
      "UpdateRequires": "No interruption"
    },
    "RetentionPeriod": {
      "Required": "No",
      "Type": "RetentionPeriod",
      "UpdateRequires": "No interruption"
    },
    "Tags": {
      "Required": "No",
      "Type": "List of [Tag",
      "Maximum": "50",
      "UpdateRequires": "No interruption"
    }
  }
}
