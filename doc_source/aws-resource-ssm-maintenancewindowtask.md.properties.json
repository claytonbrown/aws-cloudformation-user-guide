{
  "ssm-maintenancewindowtask-properties": {
    "LoggingInfo": {
      "Required": "No",
      "Type": "LoggingInfo",
      "UpdateRequires": "No interruption"
    },
    "ServiceRoleArn": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "No interruption"
    },
    "MaxErrors": {
      "Pattern": "^([1-9][0-9]*|[0]|[1-9][0-9]%|[0-9]%|100%)$",
      "Required": "Yes",
      "Maximum": "7",
      "UpdateRequires": "No interruption",
      "Minimum": "1",
      "Type": "String"
    },
    "TaskArn": {
      "Minimum": "1",
      "Required": "Yes",
      "Type": "String",
      "Maximum": "1600",
      "UpdateRequires": "No interruption"
    },
    "Name": {
      "Pattern": "^[a-zA-Z0-9_\\-.]{3,128}$",
      "Required": "No",
      "Maximum": "128",
      "UpdateRequires": "No interruption",
      "Minimum": "3",
      "Type": "String"
    },
    "TaskParameters": {
      "Required": "No",
      "Type": "Json",
      "UpdateRequires": "No interruption"
    },
    "Priority": {
      "Minimum": "0",
      "Required": "Yes",
      "Type": "Integer",
      "UpdateRequires": "No interruption"
    },
    "TaskInvocationParameters": {
      "Required": "No",
      "Type": "TaskInvocationParameters",
      "UpdateRequires": "No interruption"
    },
    "WindowId": {
      "Pattern": "^mw-[0-9a-f]{17}$",
      "Required": "No",
      "Maximum": "20",
      "UpdateRequires": "Replacement",
      "Minimum": "20",
      "Type": "String"
    },
    "TaskType": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "AUTOMATION",
        "LAMBDA",
        "RUN_COMMAND",
        "STEP_FUNCTIONS"
      ]
    },
    "MaxConcurrency": {
      "Pattern": "^([1-9][0-9]*|[1-9][0-9]%|[1-9]%|100%)$",
      "Required": "Yes",
      "Maximum": "7",
      "UpdateRequires": "No interruption",
      "Minimum": "1",
      "Type": "String"
    },
    "Targets": {
      "Required": "Yes",
      "Type": "List of [Target",
      "Maximum": "5",
      "UpdateRequires": "No interruption"
    },
    "Description": {
      "Minimum": "1",
      "Required": "No",
      "Type": "String",
      "Maximum": "128",
      "UpdateRequires": "No interruption"
    }
  }
}
