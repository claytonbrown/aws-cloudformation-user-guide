{
  "ssm.maintenancewindowtarget.Description": {
    "Minimum": "1",
    "Required": "No",
    "Type": "String",
    "Maximum": "128",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "ssm.maintenancewindowtarget.Targets": {
    "Required": "Yes",
    "Type": "List",
    "Maximum": "5",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "ssm.maintenancewindowtarget.OwnerInformation": {
    "Minimum": "1",
    "Required": "No",
    "Type": "String",
    "Maximum": "128",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "ssm.maintenancewindowtarget.WindowId": {
    "Pattern": "^mw-[0-9a-f]{17}$",
    "Required": "Yes",
    "Maximum": "20",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)\n\n#",
    "Minimum": "20",
    "Type": "String"
  },
  "ssm.maintenancewindowtarget.ResourceType": {
    "Allowed values": "INSTANCE | RESOURCE_GROUP",
    "Required": "Yes",
    "Type": "String",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "ssm.maintenancewindowtarget.Name": {
    "Pattern": "^[a-zA-Z0-9_\\-.]{3,128}$",
    "Required": "No",
    "Maximum": "128",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)",
    "Minimum": "3",
    "Type": "String"
  }
}
