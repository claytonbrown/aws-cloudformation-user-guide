{
  "aws-properties-sagemaker-workteam-cognitomemberdefinition-properties": {
    "CognitoClientId": {
      "Required": "Yes",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "128",
      "Pattern": "[\\w+]+",
      "UpdateRequires": "No interruption"
    },
    "CognitoUserGroup": {
      "Required": "Yes",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "128",
      "Pattern": "[\\p{L}\\p{M}\\p{S}\\p{N}\\p{P}]+",
      "UpdateRequires": "No interruption"
    },
    "CognitoUserPool": {
      "Required": "Yes",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "55",
      "Pattern": "[\\w-]+_[0-9a-zA-Z]+",
      "UpdateRequires": "Replacement"
    }
  }
}
