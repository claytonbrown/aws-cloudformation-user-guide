{
  "robomaker-robot-properties": {
    "Architecture": {
      "Required": "Yes",
      "Type": "String",
      "AllowedValues": [
        "ARM64",
        "ARMHF",
        "X86_64"
      ],
      "UpdateRequires": "Replacement"
    },
    "Fleet": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "GreengrassGroupId": {
      "Required": "Yes",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "1224",
      "UpdateRequires": "Replacement"
    },
    "Name": {
      "Required": "No",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "255",
      "Pattern": "[a-zA-Z0-9_\\-]*",
      "UpdateRequires": "Replacement"
    },
    "Tags": {
      "Required": "No",
      "Type": "Json",
      "UpdateRequires": "No interruption"
    }
  }
}
