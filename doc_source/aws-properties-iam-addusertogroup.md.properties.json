{
  "aws-properties-iam-addusertogroup-properties": {
    "GroupName": {
      "Required": "Yes",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "128",
      "Pattern": "[\\w+=,.@-]+",
      "UpdateRequires": "No interruption"
    },
    "Users": {
      "Required": "Yes",
      "Type": "List of String",
      "UpdateRequires": "No interruption"
    }
  }
}
