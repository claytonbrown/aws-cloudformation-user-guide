{
  "certificatemanager-certificate-properties": {
    "Tags": {
      "Required": "No",
      "Type": "List of [Tag",
      "Maximum": "50",
      "UpdateRequires": "No interruption"
    },
    "SubjectAlternativeNames": {
      "Required": "No",
      "Type": "List of String",
      "Maximum": "100",
      "UpdateRequires": "Replacement"
    },
    "ValidationMethod": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "Replacement",
      "AllowedValues": [
        "DNS",
        "EMAIL"
      ]
    },
    "DomainValidationOptions": {
      "Required": "No",
      "Type": "List of [DomainValidationOption",
      "Maximum": "100",
      "UpdateRequires": "Replacement"
    },
    "DomainName": {}
  }
}
