{
  "route53resolver-resolverendpoint-properties": {
    "Direction": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "Replacement",
      "AllowedValues": [
        "INBOUND",
        "OUTBOUND"
      ]
    },
    "Tags": {
      "Required": "No",
      "Type": "List of [Tag",
      "UpdateRequires": "No interruption"
    },
    "Name": {
      "Pattern": "(?!^[0-9]+$)([a-zA-Z0-9-_' ']+)",
      "Required": "No",
      "Type": "String",
      "Maximum": "64",
      "UpdateRequires": "No interruption"
    },
    "IpAddresses": {
      "Required": "Yes",
      "Type": "List of [IpAddressRequest",
      "Maximum": "10",
      "UpdateRequires": "No interruption"
    },
    "SecurityGroupIds": {
      "Required": "Yes",
      "Type": "List of String",
      "UpdateRequires": "Replacement"
    }
  }
}
