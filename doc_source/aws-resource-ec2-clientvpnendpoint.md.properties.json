{
  "ec2-clientvpnendpoint-properties": {
    "ServerCertificateArn": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "No interruption"
    },
    "Description": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "No interruption"
    },
    "TransportProtocol": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "Replacement",
      "AllowedValues": [
        "tcp",
        "udp"
      ]
    },
    "TagSpecifications": {
      "Required": "No",
      "Type": "List of [TagSpecification",
      "UpdateRequires": "Replacement"
    },
    "ConnectionLogOptions": {
      "Required": "Yes",
      "Type": "ConnectionLogOptions",
      "UpdateRequires": "No interruption"
    },
    "AuthenticationOptions": {
      "Required": "Yes",
      "Type": "List of [ClientAuthenticationRequest",
      "UpdateRequires": "Replacement"
    },
    "ClientCidrBlock": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "DnsServers": {
      "Required": "No",
      "Type": "List of String",
      "UpdateRequires": "No interruption"
    }
  }
}
