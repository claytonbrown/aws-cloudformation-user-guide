      "UpdateRequires": "Replacement"
    },
    "Applications": {
      "Required": "No",
      "Type": "List of [Application",
      "UpdateRequires": "Replacement"
    },
    "AutoScalingRole": {
      "Required": "No",
      "Type": "String",
      "Minimum": "0",
      "Maximum": "10280",
      "Pattern": "[\\u0020-\\uD7FF\\uE000-\\uFFFD\\uD800\\uDC00-\\uDBFF\\uDFFF\\r\\n\\t]*",
      "UpdateRequires": "Replacement"
    },
    "BootstrapActions": {
