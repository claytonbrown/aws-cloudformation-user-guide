      "UpdateRequires": "Replacement"
    },
    "KerberosAttributes": {},
    "LogUri": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "Name": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "ReleaseLabel": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "ScaleDownBehavior": {
      "Required": "No",
