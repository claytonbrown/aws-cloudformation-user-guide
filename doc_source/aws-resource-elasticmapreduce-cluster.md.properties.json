      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "Steps": {
      "Required": "No",
      "Type": "List of [StepConfig",
      "UpdateRequires": "Replacement"
    },
    "Tags": {
      "Required": "No",
      "Type": "List of [Tag",
      "UpdateRequires": "No interruption"
    },
    "VisibleToAllUsers": {
      "Required": "No",
