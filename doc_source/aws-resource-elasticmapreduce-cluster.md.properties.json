    },
    "Configurations": {
      "Required": "No",
      "Type": "List of [Configuration",
      "UpdateRequires": "Replacement"
    },
    "CustomAmiId": {
      "Required": "No",
      "Type": "String",
      "Minimum": "0",
      "Maximum": "256",
      "Pattern": "[\\u0020-\\uD7FF\\uE000-\\uFFFD\\uD800\\uDC00-\\uDBFF\\uDFFF\\r\\n\\t]*",
      "UpdateRequires": "Replacement"
    },
    "EbsRootVolumeSize": {
      "Required": "No",
