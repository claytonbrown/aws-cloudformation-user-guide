  },
  "ssm.association.instanceid": {
    "Pattern": "(^i-(\\w{8}|\\w{17})$)|(^mi-\\w{17}$)",
    "Required": "Conditional",
    "Type": "String",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "ssm.association.parameters": {
    "Required": "No",
    "Type": "Map of [ParameterValues",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "ssm.association.maxconcurrency": {
    "Pattern": "^([1-9][0-9]*|[1-9][0-9]%|[1-9]%|100%)$",
    "Required": "No",
    "Maximum": "7",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)",
    "Minimum": "1",
    "Type": "String"
  },
  "ssm.association.waitforsuccesstimeoutseconds": {
    "Required": "No",
    "Type": "Integer",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)\n\n#"
  },
  "ssm.association.synccompliance": {
    "Required": "No",
    "Type": "String",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "ssm.association.associationname": {
    "Pattern": "^[a-zA-Z0-9_\\-.]{3,128}$",
    "Required": "No",
    "Type": "String",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "ssm.association.name": {
    "Pattern": "^[a-zA-Z0-9_\\-.:/]{3,128}$",
    "Required": "Yes",
    "Type": "String",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "ssm.association.automationtargetparametername": {
    "Minimum": "1",
    "Required": "No",
    "Type": "String",
    "Maximum": "50",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "ssm.association.outputlocation": {
    "Required": "No",
    "Type": "InstanceAssociationOutputLocation",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "ssm.association.documentversion": {
    "Pattern": "([$]LATEST|[$]DEFAULT|^[1-9][0-9]*$)",
    "Required": "No",
    "Type": "String",
