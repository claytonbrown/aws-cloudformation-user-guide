{
  "cognito-userpooldomain-properties": {
    "Domain": {
      "Pattern": "^[a-z0-9](?:[a-z0-9\\-]{0,61}[a-z0-9])?$",
      "Required": "Yes",
      "Maximum": "63",
      "UpdateRequires": "Replacement",
      "Minimum": "1",
      "Type": "String"
    },
    "CustomDomainConfig": {
      "Required": "No",
      "Type": "CustomDomainConfigType",
      "UpdateRequires": "No interruption"
    },
    "UserPoolId": {
      "Pattern": "[\\w-]+_[0-9a-zA-Z]+",
      "Required": "Yes",
      "Maximum": "55",
      "UpdateRequires": "Replacement",
      "Minimum": "1",
      "Type": "String"
    }
  }
}
