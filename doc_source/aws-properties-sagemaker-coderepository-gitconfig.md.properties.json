{
  "aws-properties-sagemaker-coderepository-gitconfig-properties": {
    "SecretArn": {
      "Pattern": "arn:aws[a-z\\-]*:secretsmanager:[a-z0-9\\-]*:[0-9]{12}:secret:.*",
      "Required": "No",
      "Maximum": "2048",
      "UpdateRequires": "No interruption",
      "Minimum": "1",
      "Type": "String"
    },
    "RepositoryUrl": {
      "Pattern": "^https://([^/]+)/?(.*)$",
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "Branch": {
      "Pattern": "[^ ~^:?*\\[]+",
      "Required": "No",
      "Maximum": "1024",
      "UpdateRequires": "Replacement",
      "Minimum": "1",
      "Type": "String"
    }
  }
}
