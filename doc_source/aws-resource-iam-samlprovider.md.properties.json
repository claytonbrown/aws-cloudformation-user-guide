{
  "iam.samlprovider.Tags": {
    "Required": "No",
    "Maximum": "50",
    "UpdateRequires": "No interruption"
  },
  "iam.samlprovider.Name": {
    "Pattern": "[\\w._-]+",
    "Required": "No",
    "Maximum": "128",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)",
    "Minimum": "1",
    "Type": "String"
  },
  "iam.samlprovider.SamlMetadataDocument": {
    "Minimum": "1000",
    "Required": "Yes",
    "Maximum": "10000000",
    "UpdateRequires": "No interruption"
  }
}
