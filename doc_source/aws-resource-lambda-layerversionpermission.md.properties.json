{
  "lambda.layerversionpermission.OrganizationId": {
    "Pattern": "o-[a-z0-9]{10,32}",
    "Required": "No",
    "Maximum": "34",
    "UpdateRequires": "Replacement"
  },
  "lambda.layerversionpermission.Action": {
    "Pattern": "lambda:GetLayerVersion",
    "Required": "Yes",
    "Type": "String",
    "Maximum": "22",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)"
  },
  "lambda.layerversionpermission.LayerVersionArn": {
    "Pattern": "(arn:[a-zA-Z0-9-]+:lambda:[a-zA-Z0-9-]+:\\d{12}:layer:[a-zA-Z0-9-_]+)|[a-zA-Z0-9-_]+",
    "Required": "Yes",
    "Maximum": "140",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)",
    "Minimum": "1",
    "Type": "String"
  },
  "lambda.layerversionpermission.Principal": {
    "Pattern": "\\d{12}|\\*|arn:(aws[a-zA-Z-]*):iam::\\d{12}:root",
    "Required": "Yes",
    "UpdateRequires": "Replacement"
  }
}
