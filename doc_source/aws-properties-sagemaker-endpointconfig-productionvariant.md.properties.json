{
  "aws-properties-sagemaker-endpointconfig-productionvariant-properties": {
    "ModelName": {
      "Pattern": "^[a-zA-Z0-9](-*[a-zA-Z0-9])*",
      "Required": "Yes",
      "Type": "String",
      "Maximum": "63",
      "UpdateRequires": "Replacement"
    },
    "InitialVariantWeight": {
      "Required": "Yes",
      "Type": "Double",
      "UpdateRequires": "Replacement"
    },
    "VariantName": {
      "Pattern": "^[a-zA-Z0-9](-*[a-zA-Z0-9])*",
      "Required": "Yes",
      "Type": "String",
      "Maximum": "63",
      "UpdateRequires": "Replacement"
    },
    "InitialInstanceCount": {
      "Minimum": "1",
      "Required": "Yes",
      "Type": "Integer",
      "UpdateRequires": "Replacement"
    },
    "AcceleratorType": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "Replacement",
      "AllowedValues": [
        "ml.eia1.large",
        "ml.eia1.medium",
        "ml.eia1.xlarge"
      ]
    },
    "InstanceType": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "Replacement",
      "AllowedValues": [
        "ml.c4.2xlarge",
        "ml.c4.4xlarge",
        "ml.c4.8xlarge",
        "ml.c4.large",
        "ml.c4.xlarge",
        "ml.c5.18xlarge",
        "ml.c5.2xlarge",
        "ml.c5.4xlarge",
        "ml.c5.9xlarge",
        "ml.c5.large",
        "ml.c5.xlarge",
        "ml.m4.10xlarge",
        "ml.m4.16xlarge",
        "ml.m4.2xlarge",
        "ml.m4.4xlarge",
        "ml.m4.xlarge",
        "ml.m5.12xlarge",
        "ml.m5.24xlarge",
        "ml.m5.2xlarge",
        "ml.m5.4xlarge",
        "ml.m5.large",
        "ml.m5.xlarge",
        "ml.p2.16xlarge",
        "ml.p2.8xlarge",
        "ml.p2.xlarge",
        "ml.p3.16xlarge",
        "ml.p3.2xlarge",
        "ml.p3.8xlarge",
        "ml.t2.2xlarge",
        "ml.t2.large",
        "ml.t2.medium",
        "ml.t2.xlarge"
      ]
    }
  }
}
