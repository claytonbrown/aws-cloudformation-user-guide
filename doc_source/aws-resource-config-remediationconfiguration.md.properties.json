{
  "config-remediationconfiguration-properties": {
    "TargetType": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "SSM_DOCUMENT"
      ]
    },
    "MaximumAutomaticAttempts": {
      "Minimum": "1",
      "Required": "No",
      "Type": "Integer",
      "Maximum": "25",
      "UpdateRequires": "No interruption"
    },
    "Parameters": {
      "Required": "No",
      "Type": "Json",
      "UpdateRequires": "No interruption"
    },
    "ConfigRuleName": {
      "Pattern": ".*\\S.*",
      "Required": "Yes",
      "Maximum": "128",
      "UpdateRequires": "Replacement",
      "Minimum": "1",
      "Type": "String"
    },
    "ResourceType": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "No interruption"
    },
    "ExecutionControls": {
      "Required": "No",
      "Type": "ExecutionControls",
      "UpdateRequires": "No interruption"
    },
    "TargetId": {
      "Minimum": "1",
      "Required": "Yes",
      "Type": "String",
      "Maximum": "256",
      "UpdateRequires": "No interruption"
    },
    "RetryAttemptSeconds": {
      "Required": "No",
      "Type": "Integer",
      "UpdateRequires": "No interruption"
    },
    "Automatic": {
      "Required": "No",
      "Type": "Boolean",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "True",
        "False"
      ]
    },
    "TargetVersion": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "No interruption"
    }
  }
}
