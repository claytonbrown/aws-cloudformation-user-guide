{
  "qldb-ledger-properties": {
    "DeletionProtection": {
      "Required": "No",
      "AllowedValues": [
        "True",
        "False"
      ],
      "Type": "Boolean",
      "UpdateRequires": "No interruption"
    },
    "Name": {
      "Required": "No",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "32",
      "Pattern": "(?!^.*--)(?!^[0-9]+$)(?!^-)(?!.*-$)^[A-Za-z0-9-]+$",
      "UpdateRequires": "Replacement"
    },
    "PermissionsMode": {
      "Required": "Yes",
      "Type": "String",
      "AllowedValues": [
        "ALLOW_ALL"
      ],
      "UpdateRequires": "No interruption"
    },
    "Tags": {
      "Required": "No",
      "Type": "List of [Tag",
      "UpdateRequires": "No interruption"
    }
  }
}
