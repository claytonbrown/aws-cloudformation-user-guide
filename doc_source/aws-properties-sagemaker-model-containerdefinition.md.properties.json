{
  "aws-properties-sagemaker-model-containerdefinition-properties": {
    "ContainerHostname": {
      "Required": "No",
      "Type": "String",
      "Maximum": "63",
      "Pattern": "^[a-zA-Z0-9](-*[a-zA-Z0-9])*",
      "UpdateRequires": "Replacement"
    },
    "Environment": {
      "Required": "No",
      "Type": "Json",
      "UpdateRequires": "Replacement"
    },
    "Image": {
      "Required": "Yes",
      "Type": "String",
      "Maximum": "255",
      "Pattern": "[\\S]+",
      "UpdateRequires": "Replacement"
    },
    "ModelDataUrl": {}
  }
}
