{
  "aws-properties-robomaker-robotapplication-sourceconfig-properties": {
    "S3Bucket": {
      "Pattern": "[a-z0-9][a-z0-9\\-]*[a-z0-9]",
      "Required": "Yes",
      "Maximum": "63",
      "UpdateRequires": "No interruption",
      "Minimum": "3",
      "Type": "String"
    },
    "S3Key": {
      "Pattern": ".*",
      "Required": "Yes",
      "Maximum": "1024",
      "UpdateRequires": "No interruption",
      "Minimum": "1",
      "Type": "String"
    },
    "Architecture": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "ARM64",
        "ARMHF",
        "X86_64"
      ]
    }
  }
}
