{
  "sagemaker.endpoint.EndpointName": {
    "Pattern": "^[a-zA-Z0-9](-*[a-zA-Z0-9])*",
    "Required": "No",
    "Type": "String",
    "Maximum": "63",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)"
  },
  "sagemaker.endpoint.ExcludeRetainedVariantProperties": {
    "Required": "No",
    "Type": "List of [VariantProperty",
    "Maximum": "3",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "sagemaker.endpoint.EndpointConfigName": {
    "Pattern": "^[a-zA-Z0-9](-*[a-zA-Z0-9])*",
    "Required": "Yes",
    "Type": "String",
    "Maximum": "63",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "sagemaker.endpoint.Tags": {
    "Required": "No",
    "Maximum": "50",
    "UpdateRequires": "No interruption"
  },
  "sagemaker.endpoint.RetainAllVariantProperties": {
    "Required": "No",
    "Type": "Boolean",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)",
    "AllowedValues": [
      "True",
      "False"
    ]
  }
}
