{
  "aws-properties-cloudwatch-alarm-metric-properties": {
    "Dimensions": {
      "Required": "No",
      "Type": "List of [Dimension",
      "Maximum": "10",
      "UpdateRequires": "No interruption"
    },
    "MetricName": {
      "Required": "No",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "255",
      "UpdateRequires": "No interruption"
    },
    "Namespace": {
      "Required": "No",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "255",
      "Pattern": "[^:].*",
      "UpdateRequires": "No interruption"
    }
  }
}
