{
  "aws-properties-codepipeline-pipeline-artifactstore-properties": {
    "EncryptionKey": {
      "Required": "No",
      "Type": "EncryptionKey",
      "UpdateRequires": "No interruption"
    },
    "Location": {
      "Required": "Yes",
      "Type": "String",
      "Minimum": "3",
      "Maximum": "63",
      "Pattern": "[a-zA-Z0-9\\-\\.]+",
      "UpdateRequires": "No interruption"
    },
    "Type": {
      "Required": "Yes",
      "Type": "String",
      "AllowedValues": [
        "S3"
      ],
      "UpdateRequires": "No interruption"
    }
  }
}
