{
  "imagebuilder.imagerecipe.blockdevicemappings": {
    "Required": "No",
    "Type": "List of [InstanceBlockDeviceMapping",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)"
  },
  "imagebuilder.imagerecipe.components": {
    "Required": "Yes",
    "Type": "List of [ComponentConfiguration",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)"
  },
  "imagebuilder.imagerecipe.description": {
    "Required": "No",
    "Type": "String",
    "Minimum": "1",
    "Maximum": "1024",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)"
  },
  "imagebuilder.imagerecipe.name": {
    "Required": "Yes",
    "Type": "String",
    "Pattern": "^[-_A-Za-z-0-9][-_A-Za-z0-9 ]{1,126}[-_A-Za-z-0-9]$",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)"
  },
  "imagebuilder.imagerecipe.parentimage": {
    "Required": "Yes",
    "Type": "String",
    "Minimum": "1",
    "Maximum": "1024",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)"
  },
  "imagebuilder.imagerecipe.tags": {
    "Required": "No",
    "Type": "Map of String",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)"
  },
  "imagebuilder.imagerecipe.version": {
    "Required": "Yes",
    "Type": "String",
    "Pattern": "^[0-9]+\\.[0-9]+\\.[0-9]+$",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)\n\n#"
  }
}
