{
  "aws-properties-cloudwatch-alarm-metricstat-properties": {
    "Metric": {
      "Required": "Yes",
      "Type": "Metric",
      "UpdateRequires": "No interruption"
    },
    "Stat": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "No interruption"
    },
    "Period": {
      "Minimum": "1",
      "Required": "Yes",
      "Type": "Integer",
      "UpdateRequires": "No interruption"
    },
    "Unit": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "Bits",
        "Bits/Second",
        "Bytes",
        "Bytes/Second",
        "Count",
        "Count/Second",
        "Gigabits",
        "Gigabits/Second",
        "Gigabytes",
        "Gigabytes/Second",
        "Kilobits",
        "Kilobits/Second",
        "Kilobytes",
        "Kilobytes/Second",
        "Megabits",
        "Megabits/Second",
        "Megabytes",
        "Megabytes/Second",
        "Microseconds",
        "Milliseconds",
        "None",
        "Percent",
        "Seconds",
        "Terabits",
        "Terabits/Second",
        "Terabytes",
        "Terabytes/Second"
      ]
    }
  }
}
