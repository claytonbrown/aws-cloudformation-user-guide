{
  "lambda.alias.Name": {
    "Pattern": "(?!^[0-9]+$)([a-zA-Z0-9-_]+)",
    "Required": "Yes",
    "Maximum": "128",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)",
    "Minimum": "1",
    "Type": "String"
  },
  "lambda.alias.FunctionVersion": {
    "Pattern": "(\\$LATEST|[0-9]+)",
    "Required": "Yes",
    "Maximum": "1024",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)",
    "Minimum": "1",
    "Type": "String"
  },
  "lambda.alias.FunctionName": {
    "Pattern": "(arn:(aws[a-zA-Z-]*)?:lambda:)?([a-z]{2}(-gov)?-[a-z]+-\\d{1}:)?(\\d{12}:)?(function:)?([a-zA-Z0-9-_]+)(:(\\$LATEST|[a-zA-Z0-9-_]+))?",
    "Minimum": "1",
    "Required": "Yes",
    "Maximum": "140",
    "UpdateRequires": "Replacement"
  },
  "lambda.alias.RoutingConfig": {
    "Required": "No",
    "Type": "AliasRoutingConfiguration",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)\n\n#"
  },
  "lambda.alias.Description": {
    "Minimum": "0",
    "Required": "No",
    "Type": "String",
    "Maximum": "256",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "lambda.alias.ProvisionedConcurrencyConfig": {
    "Required": "No",
    "Type": "ProvisionedConcurrencyConfiguration",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  }
}
