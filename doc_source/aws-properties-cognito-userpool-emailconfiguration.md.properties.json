{
  "aws-properties-cognito-userpool-emailconfiguration-properties": {
    "EmailSendingAccount": {},
    "ReplyToEmailAddress": {
      "Required": "No",
      "Type": "String",
      "Pattern": "[\\p{L}\\p{M}\\p{S}\\p{N}\\p{P}]+@[\\p{L}\\p{M}\\p{S}\\p{N}\\p{P}]+",
      "UpdateRequires": "No interruption"
    },
    "SourceArn": {
      "Required": "No",
      "Type": "String",
      "Minimum": "20",
      "Maximum": "2048",
      "Pattern": "arn:[\\w+=/,.@-]+:[\\w+=/,.@-]+:([\\w+=/,.@-]*)?:[0-9]+:[\\w+=/,.@-]+(:[\\w+=/,.@-]+)?(:[\\w+=/,.@-]+)?",
      "UpdateRequires": "No interruption"
    }
  }
}
