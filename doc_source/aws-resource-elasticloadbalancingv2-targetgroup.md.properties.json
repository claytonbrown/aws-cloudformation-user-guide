{
  "elasticloadbalancingv2-targetgroup-properties": {
    "HealthyThresholdCount": {
      "Minimum": "2",
      "Required": "No",
      "Type": "Integer",
      "Maximum": "10",
      "UpdateRequires": "No interruption"
    },
    "HealthCheckIntervalSeconds": {
      "Minimum": "5",
      "Required": "No",
      "Type": "Integer",
      "Maximum": "300",
      "UpdateRequires": "No interruption"
    },
    "HealthCheckEnabled": {
      "Required": "No",
      "Type": "Boolean",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "True",
        "False"
      ]
    },
    "HealthCheckPort": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "No interruption"
    },
    "Name": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "UnhealthyThresholdCount": {
      "Minimum": "2",
      "Required": "No",
      "Type": "Integer",
      "Maximum": "10",
      "UpdateRequires": "No interruption"
    },
    "Tags": {
      "Required": "No",
      "Type": "List of [Tag",
      "UpdateRequires": "No interruption"
    },
    "TargetType": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "Replacement",
      "AllowedValues": [
        "instance",
        "ip",
        "lambda"
      ]
    },
    "HealthCheckProtocol": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "HTTP",
        "HTTPS",
        "TCP",
        "TLS"
      ]
    },
    "Targets": {
      "Required": "No",
      "Type": "List of [TargetDescription",
      "UpdateRequires": "No interruption"
    },
    "HealthCheckPath": {
      "Minimum": "1",
      "Required": "No",
      "Type": "String",
      "Maximum": "1024",
      "UpdateRequires": "No interruption"
    },
    "VpcId": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "HealthCheckTimeoutSeconds": {
      "Minimum": "2",
      "Required": "No",
      "Type": "Integer",
      "Maximum": "120",
      "UpdateRequires": "No interruption"
    },
    "TargetGroupAttributes": {
      "Required": "No",
      "Type": "List of [TargetGroupAttribute",
      "UpdateRequires": "No interruption"
    },
    "Matcher": {
      "Required": "No",
      "Type": "Matcher",
      "UpdateRequires": "No interruption"
    },
    "Protocol": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "Replacement",
      "AllowedValues": [
        "HTTP",
        "HTTPS",
        "TCP",
        "TLS"
      ]
    },
    "Port": {
      "Minimum": "1",
      "Required": "No",
      "Type": "Integer",
      "Maximum": "65535",
      "UpdateRequires": "Replacement"
    }
  }
}
