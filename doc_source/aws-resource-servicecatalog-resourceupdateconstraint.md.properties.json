{
  "servicecatalog-resourceupdateconstraint-properties": {
    "AcceptLanguage": {
      "Required": "No",
      "Type": "String",
      "Maximum": "100",
      "UpdateRequires": "No interruption"
    },
    "Description": {
      "Required": "No",
      "Type": "String",
      "Maximum": "2000",
      "UpdateRequires": "No interruption"
    },
    "PortfolioId": {
      "Required": "Yes",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "100",
      "Pattern": "^[a-zA-Z0-9_\\-]*",
      "UpdateRequires": "Replacement"
    },
    "ProductId": {
      "Required": "Yes",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "100",
      "Pattern": "^[a-zA-Z0-9_\\-]*",
      "UpdateRequires": "Replacement"
    },
    "TagUpdateOnProvisionedProduct": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "Replacement"
    }
  }
}
