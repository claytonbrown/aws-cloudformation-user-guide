{
  "aws-properties-elasticloadbalancingv2-listenerrule-redirectconfig-properties": {
    "Host": {
      "Required": "No",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "128",
      "UpdateRequires": "No interruption"
    },
    "Path": {
      "Required": "No",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "128",
      "UpdateRequires": "No interruption"
    },
    "Port": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "No interruption"
    },
    "Protocol": {
      "Required": "No",
      "Type": "String",
      "Pattern": "^(HTTPS?|#\\{protocol\\})$",
      "UpdateRequires": "No interruption"
    },
    "Query": {
      "Required": "No",
      "Type": "String",
      "Minimum": "0",
      "Maximum": "128",
      "UpdateRequires": "No interruption"
    },
    "StatusCode": {
      "Required": "Yes",
      "Type": "String",
      "AllowedValues": [
        "HTTP_301",
        "HTTP_302"
      ],
      "UpdateRequires": "No interruption"
    }
  }
}
