{
  "sagemaker-model-properties": {
    "ModelName": {
      "Pattern": "^[a-zA-Z0-9](-*[a-zA-Z0-9])*",
      "Required": "No",
      "Type": "String",
      "Maximum": "63",
      "UpdateRequires": "Replacement"
    },
    "VpcConfig": {
      "Required": "No",
      "Type": "VpcConfig",
      "UpdateRequires": "Replacement"
    },
    "Tags": {},
    "ExecutionRoleArn": {
      "Pattern": "^arn:aws[a-z\\-]*:iam::\\d{12}:role/?[a-zA-Z_0-9+=,.@\\-_/]+$",
      "Required": "Yes",
      "Maximum": "2048",
      "UpdateRequires": "Replacement",
      "Minimum": "20",
      "Type": "String"
    },
    "Containers": {
      "Required": "No",
      "Type": "List of [ContainerDefinition",
      "Maximum": "5",
      "UpdateRequires": "Replacement"
    },
    "PrimaryContainer": {
      "Required": "No",
      "Type": "ContainerDefinition",
      "UpdateRequires": "Replacement"
    }
  }
}
