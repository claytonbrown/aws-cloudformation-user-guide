{
  "ec2-trafficmirrorfilterrule-properties": {
    "DestinationCidrBlock": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "No interruption"
    },
    "RuleNumber": {
      "Required": "Yes",
      "Type": "Integer",
      "UpdateRequires": "No interruption"
    },
    "Protocol": {
      "Required": "No",
      "Type": "Integer",
      "UpdateRequires": "No interruption"
    },
    "Description": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "No interruption"
    },
    "TrafficDirection": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "egress",
        "ingress"
      ]
    },
    "DestinationPortRange": {
      "Required": "No",
      "Type": "TrafficMirrorPortRangeRequest",
      "UpdateRequires": "No interruption"
    },
    "RuleAction": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "accept",
        "reject"
      ]
    },
    "SourceCidrBlock": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "No interruption"
    },
    "TrafficMirrorFilterId": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "SourcePortRange": {
      "Required": "No",
      "Type": "TrafficMirrorPortRangeRequest",
      "UpdateRequires": "No interruption"
    }
  }
}
