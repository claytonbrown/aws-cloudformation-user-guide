{
  "kinesisanalytics-application-properties": {
    "ApplicationCode": {
      "Required": "No",
      "Type": "String",
      "Minimum": "0",
      "Maximum": "102400",
      "UpdateRequires": "No interruption"
    },
    "ApplicationDescription": {
      "Required": "No",
      "Type": "String",
      "Minimum": "0",
      "Maximum": "1024",
      "UpdateRequires": "No interruption"
    },
    "ApplicationName": {
      "Required": "No",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "128",
      "Pattern": "[a-zA-Z0-9_.-]+",
      "UpdateRequires": "Replacement"
    },
    "Inputs": {
      "Required": "Yes",
      "Type": "List of [Input",
      "UpdateRequires": "No interruption"
    }
  }
}
