{
  "opsworks-layer-properties": {
    "StackId": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "LifecycleEventConfiguration": {
      "Required": "No",
      "Type": "LifecycleEventConfiguration",
      "UpdateRequires": "No interruption"
    },
    "Type": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "Replacement",
      "AllowedValues": [
        "aws-flow-ruby",
        "custom",
        "db-master",
        "ecs-cluster",
        "java-app",
        "lb",
        "memcached",
        "monitoring-master",
        "nodejs-app",
        "php-app",
        "rails-app",
        "web"
      ]
    },
    "Packages": {
      "Required": "No",
      "Type": "List of String",
      "UpdateRequires": "No interruption"
    },
    "Name": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "No interruption"
    },
    "LoadBasedAutoScaling": {
      "Required": "No",
      "Type": "LoadBasedAutoScaling",
      "UpdateRequires": "No interruption"
    },
    "UseEbsOptimizedInstances": {
      "Required": "No",
      "Type": "Boolean",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "True",
        "False"
      ]
    },
    "CustomJson": {
      "Required": "No",
      "Type": "Json",
      "UpdateRequires": "No interruption"
    },
    "CustomRecipes": {
      "Required": "No",
      "Type": "Recipes",
      "UpdateRequires": "No interruption"
    },
    "AutoAssignPublicIps": {
      "Required": "Yes",
      "Type": "Boolean",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "True",
        "False"
      ]
    },
    "EnableAutoHealing": {
      "Required": "Yes",
      "Type": "Boolean",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "True",
        "False"
      ]
    },
    "CustomInstanceProfileArn": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "No interruption"
    },
    "Tags": {
      "Required": "No",
      "Type": "List of [Tag",
      "UpdateRequires": "No interruption"
    },
    "VolumeConfigurations": {
      "Required": "No",
      "Type": "List of [VolumeConfiguration",
      "UpdateRequires": "No interruption"
    },
    "Attributes": {
      "Required": "No",
      "Type": "Map of String",
      "UpdateRequires": "No interruption"
    },
    "InstallUpdatesOnBoot": {
      "Required": "No",
      "Type": "Boolean",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "True",
        "False"
      ]
    },
    "AutoAssignElasticIps": {
      "Required": "Yes",
      "Type": "Boolean",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "True",
        "False"
      ]
    },
    "CustomSecurityGroupIds": {
      "Required": "No",
      "Type": "List of String",
      "UpdateRequires": "No interruption"
    },
    "Shortname": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "No interruption"
    }
  }
}
