{
  "imagebuilder.imagepipeline.ImageRecipeArn": {
    "Required": "Yes",
    "Type": "String",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "imagebuilder.imagepipeline.Description": {
    "Minimum": "1",
    "Required": "No",
    "Type": "String",
    "Maximum": "1024",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "imagebuilder.imagepipeline.Schedule": {
    "Required": "No",
    "Type": "Schedule",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "imagebuilder.imagepipeline.Name": {
    "Pattern": "^[-_A-Za-z-0-9][-_A-Za-z0-9 ]{1,126}[-_A-Za-z-0-9]$",
    "Required": "Yes",
    "Type": "String",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)"
  },
  "imagebuilder.imagepipeline.Tags": {
    "Required": "No",
    "Type": "Map of String",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)\n\n#"
  },
  "imagebuilder.imagepipeline.InfrastructureConfigurationArn": {
    "Required": "Yes",
    "Type": "String",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "imagebuilder.imagepipeline.EnhancedImageMetadataEnabled": {
    "Required": "No",
    "Type": "Boolean",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)",
    "AllowedValues": [
      "True",
      "False"
    ]
  },
  "imagebuilder.imagepipeline.ImageTestsConfiguration": {
    "Required": "No",
    "Type": "ImageTestsConfiguration",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "imagebuilder.imagepipeline.Status": {
    "Allowed values": "DISABLED | ENABLED",
    "Required": "No",
    "Type": "String",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "imagebuilder.imagepipeline.DistributionConfigurationArn": {
    "Required": "No",
    "Type": "String",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  }
}
