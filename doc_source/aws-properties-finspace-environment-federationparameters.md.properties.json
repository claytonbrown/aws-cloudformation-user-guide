{
  "finspace.environment.federationparameters.SamlMetadataDocument": {
    "Pattern": ".*",
    "Required": "Yes",
    "Maximum": "10000000",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)",
    "Minimum": "1000",
    "Type": "String"
  },
  "finspace.environment.federationparameters.SamlMetadataURL": {
    "Pattern": "^https?://[-a-zA-Z0-9+&@#/%?=~_|!:,.;]*[-a-zA-Z0-9+&@#/%=~_|]",
    "Required": "Yes",
    "Maximum": "1000",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)",
    "Minimum": "1",
    "Type": "String"
  },
  "finspace.environment.federationparameters.FederationProviderName": {
    "Pattern": "[^_\\p{Z}][\\p{L}\\p{M}\\p{S}\\p{N}\\p{P}][^_\\p{Z}]+",
    "Required": "Yes",
    "Maximum": "32",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)",
    "Minimum": "1",
    "Type": "String"
  }
}
