{
  "aws-properties-ecs-taskdefinition-linuxparameters-properties": {
    "Tmpfs": {
      "Required": "No",
      "Type": "List",
      "UpdateRequires": "Replacement"
    },
    "InitProcessEnabled": {
      "Required": "No",
      "Type": "Boolean",
      "UpdateRequires": "Replacement",
      "AllowedValues": [
        "True",
        "False"
      ]
    },
    "Devices": {
      "Required": "No",
      "Type": "List of [Device",
      "UpdateRequires": "Replacement"
    },
    "Capabilities": {
      "Required": "No",
      "Type": "KernelCapabilities",
      "UpdateRequires": "Replacement"
    },
    "SharedMemorySize": {
      "Required": "No",
      "Type": "Integer",
      "UpdateRequires": "Replacement"
    }
  }
}
