{
  "codepipeline-customactiontype-properties": {
    "Category": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "Replacement",
      "AllowedValues": [
        "Approval",
        "Build",
        "Deploy",
        "Invoke",
        "Source",
        "Test"
      ]
    },
    "Settings": {
      "Required": "No",
      "Type": "Settings",
      "UpdateRequires": "Replacement"
    },
    "Tags": {
      "Required": "No",
      "Type": "List of [Tag",
      "UpdateRequires": "No interruption"
    },
    "ConfigurationProperties": {
      "Required": "No",
      "Type": "List",
      "Maximum": "10",
      "UpdateRequires": "Replacement"
    },
    "Version": {
      "Pattern": "[0-9A-Za-z_-]+",
      "Required": "Yes",
      "Maximum": "9",
      "UpdateRequires": "Replacement",
      "Minimum": "1",
      "Type": "String"
    },
    "Provider": {
      "Pattern": "[0-9A-Za-z_-]+",
      "Required": "Yes",
      "Maximum": "25",
      "UpdateRequires": "Replacement",
      "Minimum": "1",
      "Type": "String"
    },
    "InputArtifactDetails": {
      "Required": "Yes",
      "Type": "ArtifactDetails",
      "UpdateRequires": "Replacement"
    },
    "OutputArtifactDetails": {
      "Required": "Yes",
      "Type": "ArtifactDetails",
      "UpdateRequires": "Replacement"
    }
  }
}
