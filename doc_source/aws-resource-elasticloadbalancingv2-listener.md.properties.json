{
  "elasticloadbalancingv2-listener-properties": {
    "Protocol": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "HTTP",
        "HTTPS",
        "TCP",
        "TLS"
      ]
    },
    "DefaultActions": {
      "Required": "Yes",
      "Type": "List of [Action",
      "UpdateRequires": "No interruption"
    },
    "SslPolicy": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "No interruption"
    },
    "Certificates": {
      "Required": "Conditional",
      "Type": "List of [Certificate",
      "UpdateRequires": "No interruption"
    },
    "LoadBalancerArn": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "Port": {
      "Minimum": "1",
      "Required": "Yes",
      "Type": "Integer",
      "Maximum": "65535",
      "UpdateRequires": "No interruption"
    }
  }
}
