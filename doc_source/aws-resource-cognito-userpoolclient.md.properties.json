{
  "cognito-userpoolclient-properties": {
    "WriteAttributes": {
      "Required": "No",
      "Type": "List of String",
      "UpdateRequires": "No interruption"
    },
    "CallbackURLs": {
      "Required": "No",
      "Type": "List of String",
      "Maximum": "100",
      "UpdateRequires": "No interruption"
    },
    "AllowedOAuthScopes": {
      "Required": "No",
      "Type": "List of String",
      "Maximum": "25",
      "UpdateRequires": "No interruption"
    },
    "UserPoolId": {
      "Pattern": "[\\w-]+_[0-9a-zA-Z]+",
      "Required": "Yes",
      "Maximum": "55",
      "UpdateRequires": "Replacement",
      "Minimum": "1",
      "Type": "String"
    },
    "AllowedOAuthFlowsUserPoolClient": {
      "Required": "No",
      "Type": "Boolean",
      "UpdateRequires": "Replacement",
      "AllowedValues": [
        "True",
        "False"
      ]
    },
    "AnalyticsConfiguration": {
      "Required": "No",
      "Type": "AnalyticsConfiguration",
      "UpdateRequires": "No interruption"
    },
    "LogoutURLs": {
      "Required": "No",
      "Type": "List of String",
      "Maximum": "100",
      "UpdateRequires": "No interruption"
    },
    "AllowedOAuthFlows": {
      "Required": "No",
      "Type": "List of String",
      "Maximum": "3",
      "UpdateRequires": "No interruption"
    },
    "GenerateSecret": {
      "Required": "No",
      "Type": "Boolean",
      "UpdateRequires": "Replacement",
      "AllowedValues": [
        "True",
        "False"
      ]
    },
    "ExplicitAuthFlows": {
      "Required": "No",
      "Type": "List of String",
      "UpdateRequires": "No interruption"
    },
    "ReadAttributes": {
      "Required": "No",
      "Type": "List of String",
      "UpdateRequires": "No interruption"
    },
    "SupportedIdentityProviders": {
      "Required": "No",
      "Type": "List of String",
      "UpdateRequires": "No interruption"
    },
    "DefaultRedirectURI": {
      "Pattern": "[\\p{L}\\p{M}\\p{S}\\p{N}\\p{P}]+",
      "Required": "No",
      "Maximum": "1024",
      "UpdateRequires": "No interruption",
      "Minimum": "1",
      "Type": "String"
    },
    "RefreshTokenValidity": {
      "Minimum": "0",
      "Required": "No",
      "Type": "Double",
      "Maximum": "3650",
      "UpdateRequires": "No interruption"
    },
    "ClientName": {
      "Pattern": "[\\w\\s+=,.@-]+",
      "Required": "No",
      "Maximum": "128",
      "UpdateRequires": "No interruption",
      "Minimum": "1",
      "Type": "String"
    }
  }
}
