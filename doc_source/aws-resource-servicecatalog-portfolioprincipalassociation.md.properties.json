{
  "servicecatalog-portfolioprincipalassociation-properties": {
    "AcceptLanguage": {
      "Required": "No",
      "Type": "String",
      "Maximum": "100",
      "UpdateRequires": "Replacement"
    },
    "PortfolioId": {
      "Required": "Yes",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "100",
      "Pattern": "^[a-zA-Z0-9_\\-]*",
      "UpdateRequires": "Replacement"
    },
    "PrincipalARN": {
      "Required": "Yes",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "1000",
      "UpdateRequires": "Replacement"
    },
    "PrincipalType": {
      "Required": "Yes",
      "Type": "String",
      "AllowedValues": [
        "IAM"
      ],
      "UpdateRequires": "Replacement"
    }
  }
}
