{
  "iotevents-detectormodel-properties": {
    "DetectorModelDefinition": {
      "Required": "No",
      "Type": "DetectorModelDefinition",
      "UpdateRequires": "No interruption"
    },
    "DetectorModelDescription": {
      "Required": "No",
      "Type": "String",
      "Maximum": "128",
      "UpdateRequires": "No interruption"
    },
    "DetectorModelName": {
      "Required": "No",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "128",
      "Pattern": "^[a-zA-Z0-9_-]+$",
      "UpdateRequires": "Replacement"
    },
    "Key": {
      "Required": "No",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "128",
      "Pattern": "^((`[\\w\\- ]+`)|([\\w\\-]+))(\\.((`[\\w- ]+`)|([\\w\\-]+)))*$",
      "UpdateRequires": "Replacement"
    },
    "RoleArn": {
      "Required": "No",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "2048",
      "UpdateRequires": "No interruption"
    },
    "Tags": {
      "Required": "No",
      "Type": "List of [Tag",
      "UpdateRequires": "No interruption"
    }
  }
}
