{
  "directoryservice.microsoftad.CreateAlias": {
    "Required": "No",
    "Type": "Boolean",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)",
    "AllowedValues": [
      "True",
      "False"
    ]
  },
  "directoryservice.microsoftad.Name": {
    "Pattern": "^([a-zA-Z0-9]+[\\\\.-])+([a-zA-Z0-9])+$",
    "Required": "Yes",
    "Type": "String",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)"
  },
  "directoryservice.microsoftad.VpcSettings": {
    "Required": "Yes",
    "Type": "VpcSettings",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)\n\n#"
  },
  "directoryservice.microsoftad.Password": {
    "Pattern": "(?=^.{8,64}$)((?=.*\\d)(?=.*[A-Z])(?=.*[a-z])|(?=.*\\d)(?=.*[^A-Za-z0-9\\s])(?=.*[a-z])|(?=.*[^A-Za-z0-9\\s])(?=.*[A-Z])(?=.*[a-z])|(?=.*\\d)(?=.*[A-Z])(?=.*[^A-Za-z0-9\\s]))^.*",
    "Required": "Yes",
    "UpdateRequires": "Replacement"
  },
  "directoryservice.microsoftad.Edition": {
    "Allowed values": "Enterprise | Standard",
    "Required": "No",
    "Type": "String",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)"
  },
  "directoryservice.microsoftad.ShortName": {
    "Pattern": "^[^\\\\/:*?\"<>|.]+[^\\\\/:*?\"<>|]*$",
    "Required": "No",
    "Type": "String",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)"
  },
  "directoryservice.microsoftad.EnableSso": {
    "Required": "No",
    "Type": "Boolean",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)",
    "AllowedValues": [
      "True",
      "False"
    ]
  }
}
