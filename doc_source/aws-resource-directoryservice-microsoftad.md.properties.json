{
  "directoryservice-microsoftad-properties": {
    "Password": {
      "Pattern": "(?=^.{8,64}$)((?=.*\\d)(?=.*[A-Z])(?=.*[a-z])|(?=.*\\d)(?=.*[^A-Za-z0-9\\s])(?=.*[a-z])|(?=.*[^A-Za-z0-9\\s])(?=.*[A-Z])(?=.*[a-z])|(?=.*\\d)(?=.*[A-Z])(?=.*[^A-Za-z0-9\\s]))^.*",
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "Name": {
      "Pattern": "^([a-zA-Z0-9]+[\\\\.-])+([a-zA-Z0-9])+$",
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "VpcSettings": {
      "Required": "Yes",
      "Type": "VpcSettings",
      "UpdateRequires": "Replacement"
    },
    "Edition": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "Replacement",
      "AllowedValues": [
        "Enterprise",
        "Standard"
      ]
    },
    "EnableSso": {
      "Required": "No",
      "Type": "Boolean",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "True",
        "False"
      ]
    },
    "ShortName": {
      "Pattern": "^[^\\\\/:*?\\\"\\<\\>|.]+[^\\\\/:*?\\\"<>|]*$",
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "CreateAlias": {
      "Required": "No",
      "Type": "Boolean",
      "UpdateRequires": "Replacement",
      "AllowedValues": [
        "True",
        "False"
      ]
    }
  }
}
