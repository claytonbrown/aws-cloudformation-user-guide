{
  "logs-logstream-properties": {
    "LogGroupName": {
      "Required": "Yes",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "512",
      "Pattern": "[\\.\\-_/#A-Za-z0-9]+",
      "UpdateRequires": "Replacement"
    },
    "LogStreamName": {
      "Required": "No",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "512",
      "Pattern": "[^:*]*",
      "UpdateRequires": "Replacement"
    }
  }
}
