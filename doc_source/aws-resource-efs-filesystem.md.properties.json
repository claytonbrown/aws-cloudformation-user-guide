{
  "efs.filesystem.FileSystemTags": {
    "Required": "No",
    "Type": "List of [ElasticFileSystemTag",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "efs.filesystem.LifecyclePolicies": {
    "Required": "No",
    "Type": "List of [LifecyclePolicy",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "efs.filesystem.Encrypted": {
    "Required": "Conditional",
    "Type": "Boolean",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)",
    "AllowedValues": [
      "True",
      "False"
    ]
  },
  "efs.filesystem.ThroughputMode": {
    "Required": "No",
    "UpdateRequires": "No interruption"
  },
  "efs.filesystem.KmsKeyId": {
    "Minimum": "1",
    "Required": "No",
    "Type": "String",
    "Maximum": "2048",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)"
  },
  "efs.filesystem.PerformanceMode": {
    "Allowed values": "generalPurpose | maxIO",
    "Required": "No",
    "Type": "String",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)"
  },
  "efs.filesystem.ProvisionedThroughputInMibps": {
    "Required": "Conditional",
    "UpdateRequires": "No interruption"
  },
  "efs.filesystem.FileSystemPolicy": {
    "Required": "No",
    "UpdateRequires": "No interruption"
  }
}
