{
  "efs-filesystem-properties": {
    "FileSystemTags": {
      "Required": "No",
      "Type": "List of [ElasticFileSystemTag",
      "UpdateRequires": "No interruption"
    },
    "ThroughputMode": {},
    "Encrypted": {
      "Required": "Conditional",
      "Type": "Boolean",
      "UpdateRequires": "Replacement",
      "AllowedValues": [
        "True",
        "False"
      ]
    },
    "PerformanceMode": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "Replacement",
      "AllowedValues": [
        "generalPurpose",
        "maxIO"
      ]
    },
    "KmsKeyId": {
      "Minimum": "1",
      "Required": "No",
      "Type": "String",
      "Maximum": "2048",
      "UpdateRequires": "Replacement"
    },
    "ProvisionedThroughputInMibps": {}
  }
}
