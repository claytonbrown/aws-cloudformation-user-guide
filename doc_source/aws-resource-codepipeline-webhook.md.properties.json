{
  "codepipeline-webhook-properties": {
    "AuthenticationConfiguration": {
      "Required": "Yes",
      "Type": "WebhookAuthConfiguration",
      "UpdateRequires": "No interruption"
    },
    "Name": {
      "Pattern": "[A-Za-z0-9.@\\-_]+",
      "Required": "No",
      "Maximum": "100",
      "UpdateRequires": "Replacement",
      "Minimum": "1",
      "Type": "String"
    },
    "TargetPipelineVersion": {
      "Required": "Yes",
      "Type": "Integer",
      "UpdateRequires": "No interruption"
    },
    "RegisterWithThirdParty": {
      "Required": "No",
      "Type": "Boolean",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "True",
        "False"
      ]
    },
    "Authentication": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "GITHUB_HMAC",
        "IP",
        "UNAUTHENTICATED"
      ]
    },
    "TargetPipeline": {
      "Pattern": "[A-Za-z0-9.@\\-_]+",
      "Required": "Yes",
      "Maximum": "100",
      "UpdateRequires": "No interruption",
      "Minimum": "1",
      "Type": "String"
    },
    "TargetAction": {
      "Pattern": "[A-Za-z0-9.@\\-_]+",
      "Required": "Yes",
      "Maximum": "100",
      "UpdateRequires": "No interruption",
      "Minimum": "1",
      "Type": "String"
    },
    "Filters": {
      "Required": "Yes",
      "Type": "List of [WebhookFilterRule",
      "Maximum": "5",
      "UpdateRequires": "No interruption"
    }
  }
}
