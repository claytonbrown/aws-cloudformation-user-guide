{
  "ec2-vpcendpoint-properties": {
    "VpcId": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "ServiceName": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "SubnetIds": {
      "Required": "No",
      "Type": "List of String",
      "UpdateRequires": "No interruption"
    },
    "SecurityGroupIds": {
      "Required": "No",
      "Type": "List of String",
      "UpdateRequires": "No interruption"
    },
    "PolicyDocument": {
      "Required": "No",
      "Type": "Json",
      "UpdateRequires": "No interruption"
    },
    "RouteTableIds": {
      "Required": "No",
      "Type": "List of String",
      "UpdateRequires": "No interruption"
    },
    "PrivateDnsEnabled": {
      "Required": "No",
      "Type": "Boolean",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "True",
        "False"
      ]
    },
    "VpcEndpointType": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "Replacement",
      "AllowedValues": [
        "Gateway",
        "Interface"
      ]
    }
  }
}
