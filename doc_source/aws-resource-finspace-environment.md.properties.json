    "Required": "Yes",
    "Maximum": "255",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)\n\n#",
    "Minimum": "1",
    "Type": "String"
  },
  "finspace.environment.Description": {
    "Pattern": "^[a-zA-Z0-9. ]{1,1000}$",
    "Required": "No",
    "Maximum": "1000",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)",
    "Minimum": "1",
    "Type": "String"
  },
  "finspace.environment.FederationParameters": {
    "Required": "No",
    "Type": "FederationParameters",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "finspace.environment.FederationMode": {
    "Allowed values": "FEDERATED | LOCAL",
    "Required": "No",
    "Type": "String",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "finspace.environment.KmsKeyId": {
    "Pattern": "^[a-zA-Z-0-9-:\\/]*$",
    "Required": "No",
    "Maximum": "1000",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)",
    "Minimum": "1",
    "Type": "String"
  }
}
