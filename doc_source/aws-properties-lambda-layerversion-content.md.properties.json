{
  "aws-properties-lambda-layerversion-content-properties": {
    "S3Bucket": {
      "Required": "Yes",
      "Type": "String",
      "Minimum": "3",
      "Maximum": "63",
      "Pattern": "^[0-9A-Za-z\\.\\-_]*(?<!\\.)$",
      "UpdateRequires": "Replacement"
    },
    "S3Key": {
      "Required": "Yes",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "1024",
      "UpdateRequires": "Replacement"
    },
    "S3ObjectVersion": {
      "Required": "No",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "1024",
      "UpdateRequires": "Replacement"
    }
  }
}
