{
  "iotanalytics-dataset-properties": {
    "Actions": {
      "Required": "Yes",
      "Type": "List of [Action",
      "Maximum": "1",
      "UpdateRequires": "No interruption"
    },
    "ContentDeliveryRules": {
      "Required": "No",
      "Type": "List of [DatasetContentDeliveryRule",
      "Maximum": "20",
      "UpdateRequires": "No interruption"
    },
    "DatasetName": {
      "Required": "No",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "128",
      "Pattern": "^[a-zA-Z0-9_]+$",
      "UpdateRequires": "Replacement"
    },
    "RetentionPeriod": {
      "Required": "No",
      "Type": "RetentionPeriod",
      "UpdateRequires": "No interruption"
    },
    "Tags": {
      "Required": "No",
      "Type": "List of [Tag",
      "Maximum": "50",
      "UpdateRequires": "No interruption"
    },
    "Triggers": {
      "Required": "No",
      "Type": "List of [Trigger",
      "Maximum": "5",
      "UpdateRequires": "No interruption"
    },
    "VersioningConfiguration": {
      "Required": "No",
      "Type": "VersioningConfiguration",
      "UpdateRequires": "No interruption"
    }
  }
}
