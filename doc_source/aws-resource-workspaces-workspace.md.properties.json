{
  "workspaces-workspace-properties": {
    "BundleId": {
      "Required": "Yes",
      "Type": "String",
      "Pattern": "^wsb-[0-9a-z]{8,63}$"
    },
    "DirectoryId": {
      "Required": "Yes",
      "Type": "String",
      "Pattern": "^d-[0-9a-f]{8,63}$"
    },
    "RootVolumeEncryptionEnabled": {
      "Required": "No",
      "AllowedValues": [
        "True",
        "False"
      ],
      "Type": "Boolean"
    },
    "Tags": {
      "Required": "No",
      "Type": "List of [Tag",
      "UpdateRequires": "No interruption"
    },
    "UserName": {
      "Required": "Yes",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "63",
      "UpdateRequires": "Replacement"
    },
    "UserVolumeEncryptionEnabled": {
      "Required": "No",
      "AllowedValues": [
        "True",
        "False"
      ],
      "Type": "Boolean"
    },
    "VolumeEncryptionKey": {
      "Required": "No",
      "Type": "String"
    },
    "WorkspaceProperties": {
      "Required": "No",
      "Type": "WorkspaceProperties",
      "UpdateRequires": "No interruption"
    }
  }
}
