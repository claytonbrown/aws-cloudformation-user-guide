{
  "cognito-userpoolresourceserver-properties": {
    "Scopes": {
      "Required": "No",
      "Type": "List of [ResourceServerScopeType",
      "Maximum": "100",
      "UpdateRequires": "No interruption"
    },
    "Identifier": {
      "Pattern": "[\\x21\\x23-\\x5B\\x5D-\\x7E]+",
      "Required": "Yes",
      "Maximum": "256",
      "UpdateRequires": "Replacement",
      "Minimum": "1",
      "Type": "String"
    },
    "Name": {
      "Pattern": "[\\w\\s+=,.@-]+",
      "Required": "Yes",
      "Maximum": "256",
      "UpdateRequires": "No interruption",
      "Minimum": "1",
      "Type": "String"
    },
    "UserPoolId": {
      "Pattern": "[\\w-]+_[0-9a-zA-Z]+",
      "Required": "Yes",
      "Maximum": "55",
      "UpdateRequires": "Replacement",
      "Minimum": "1",
      "Type": "String"
    }
  }
}
