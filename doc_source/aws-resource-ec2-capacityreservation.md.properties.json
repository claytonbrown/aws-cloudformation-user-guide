{
  "ec2-capacityreservation-properties": {
    "AvailabilityZone": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "EbsOptimized": {
      "Required": "No",
      "AllowedValues": [
        "True",
        "False"
      ],
      "Type": "Boolean",
      "UpdateRequires": "Replacement"
    },
    "EndDate": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "No interruption"
    },
    "EndDateType": {
      "Required": "No",
      "Type": "String",
      "AllowedValues": [
        "limited",
        "unlimited"
      ],
      "UpdateRequires": "No interruption"
    },
    "EphemeralStorage": {
      "Required": "No",
      "AllowedValues": [
        "True",
        "False"
      ],
      "Type": "Boolean",
      "UpdateRequires": "Replacement"
    },
    "InstanceCount": {
      "Required": "Yes",
      "Type": "Integer",
      "UpdateRequires": "No interruption"
    },
    "InstanceMatchCriteria": {
      "Required": "No",
      "Type": "String",
      "AllowedValues": [
        "open",
        "targeted"
      ],
      "UpdateRequires": "Replacement"
    },
    "InstancePlatform": {
      "Required": "Yes",
      "Type": "String",
      "AllowedValues": [
        "Linux/UNIX",
        "Red Hat Enterprise Linux",
        "SUSE Linux",
        "Windows",
        "Windows with SQL Server",
        "Windows with SQL Server Enterprise",
        "Windows with SQL Server Standard",
        "Windows with SQL Server Web"
      ],
      "UpdateRequires": "Replacement"
    },
    "InstanceType": {},
    "TagSpecifications": {
      "Required": "No",
      "Type": "List of [TagSpecification",
      "UpdateRequires": "Replacement"
    },
    "Tenancy": {
      "Required": "No",
      "Type": "String",
      "AllowedValues": [
        "dedicated",
        "default"
      ],
      "UpdateRequires": "Replacement"
    }
  }
}
