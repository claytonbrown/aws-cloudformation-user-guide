{
  "ssm-document-properties": {
    "Content": {
      "Required": "Yes",
      "Type": "Json",
      "Minimum": "1",
      "UpdateRequires": "Replacement"
    },
    "DocumentType": {
      "Required": "No",
      "Type": "String",
      "AllowedValues": [
        "Automation",
        "Command",
        "Package",
        "Policy",
        "Session"
      ],
      "UpdateRequires": "Replacement"
    },
    "Tags": {
      "Required": "No",
      "Type": "List of [Tag",
      "Maximum": "1000",
      "UpdateRequires": "No interruption"
    }
  }
}
