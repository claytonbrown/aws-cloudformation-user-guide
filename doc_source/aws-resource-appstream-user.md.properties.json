{
  "appstream-user-properties": {
    "AuthenticationType": {
      "Required": "Yes",
      "Type": "String",
      "AllowedValues": [
        "API",
        "SAML",
        "USERPOOL"
      ],
      "UpdateRequires": "Replacement"
    },
    "FirstName": {
      "Required": "No",
      "Type": "String",
      "Maximum": "2048",
      "Pattern": "^[A-Za-z0-9_\\-\\s]+$",
      "UpdateRequires": "Replacement"
    },
    "LastName": {
      "Required": "No",
      "Type": "String",
      "Maximum": "2048",
      "Pattern": "^[A-Za-z0-9_\\-\\s]+$",
      "UpdateRequires": "Replacement"
    },
    "MessageAction": {
      "Required": "No",
      "Type": "String",
      "AllowedValues": [
        "RESEND",
        "SUPPRESS"
      ],
      "UpdateRequires": "Replacement"
    },
    "UserName": {
      "Required": "Yes",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "128",
      "Pattern": "[\\p{L}\\p{M}\\p{S}\\p{N}\\p{P}]+",
      "UpdateRequires": "Replacement"
    }
  }
}
