{
  "appstream-user-properties": {
    "UserName": {
      "Pattern": "[\\p{L}\\p{M}\\p{S}\\p{N}\\p{P}]+",
      "Required": "Yes",
      "Maximum": "128",
      "UpdateRequires": "Replacement",
      "Minimum": "1",
      "Type": "String"
    },
    "LastName": {
      "Pattern": "^[A-Za-z0-9_\\-\\s]+$",
      "Required": "No",
      "Type": "String",
      "Maximum": "2048",
      "UpdateRequires": "Replacement"
    },
    "MessageAction": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "Replacement",
      "AllowedValues": [
        "RESEND",
        "SUPPRESS"
      ]
    },
    "FirstName": {
      "Pattern": "^[A-Za-z0-9_\\-\\s]+$",
      "Required": "No",
      "Type": "String",
      "Maximum": "2048",
      "UpdateRequires": "Replacement"
    },
    "AuthenticationType": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "Replacement"
    }
  }
}
