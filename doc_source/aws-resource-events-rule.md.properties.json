{
  "events-rule-properties": {
    "ScheduleExpression": {
      "Required": "Conditional",
      "Type": "String",
      "Maximum": "256",
      "UpdateRequires": "No interruption"
    },
    "Description": {
      "Required": "No",
      "Type": "String",
      "Maximum": "512",
      "UpdateRequires": "No interruption"
    },
    "RoleArn": {
      "Minimum": "1",
      "Required": "No",
      "Type": "String",
      "Maximum": "1600",
      "UpdateRequires": "No interruption"
    },
    "\"EventPattern\": { \"source\": [ \"aws.ec2\" ], \"detail-type\": [ \"EC2 Instance State-change Notification\" ] }": {
      "Required": "Conditional",
      "Type": "Json",
      "UpdateRequires": "No interruption"
    },
    "Targets": {},
    "State": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "DISABLED",
        "ENABLED"
      ]
    },
    "EventPattern": {},
    "Name": {
      "Pattern": "[\\.\\-_A-Za-z0-9]+",
      "Required": "No",
      "Maximum": "64",
      "UpdateRequires": "Replacement",
      "Minimum": "1",
      "Type": "String"
    }
  }
}
