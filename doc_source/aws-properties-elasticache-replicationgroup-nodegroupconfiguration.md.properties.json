{
  "aws-properties-elasticache-replicationgroup-nodegroupconfiguration-properties": {
    "ReplicaCount": {
      "Required": "No",
      "Type": "Integer",
      "UpdateRequires": "Replacement"
    },
    "Slots": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "NodeGroupId": {
      "Pattern": "\\d+",
      "Minimum": "1",
      "Required": "No",
      "Type": "String",
      "Maximum": "4"
    },
    "PrimaryAvailabilityZone": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "ReplicaAvailabilityZones": {
      "Required": "No",
      "Type": "List of String",
      "UpdateRequires": "Replacement"
    }
  }
}
