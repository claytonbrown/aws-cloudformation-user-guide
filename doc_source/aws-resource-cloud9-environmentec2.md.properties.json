{
  "cloud9-environmentec2-properties": {
    "Name": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "Repositories": {
      "Required": "No",
      "Type": "List of [Repository",
      "UpdateRequires": "Replacement"
    },
    "OwnerArn": {
      "Pattern": "arn:aws:(iam|sts)::\\d+:(root|user|federated-user|assumed-role)\\/?\\S*",
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "SubnetId": {
      "Minimum": "5",
      "Required": "No",
      "Type": "String",
      "Maximum": "30",
      "UpdateRequires": "Replacement"
    },
    "AutomaticStopTimeMinutes": {
      "Required": "No",
      "Type": "Integer",
      "Maximum": "20160",
      "UpdateRequires": "Replacement"
    },
    "InstanceType": {
      "Pattern": "^[a-z][1-9][.][a-z0-9]+$",
      "Required": "Yes",
      "Maximum": "20",
      "UpdateRequires": "Replacement",
      "Minimum": "5",
      "Type": "String"
    },
    "Description": {
      "Required": "No",
      "Type": "String",
      "Maximum": "200",
      "UpdateRequires": "Replacement"
    }
  }
}
