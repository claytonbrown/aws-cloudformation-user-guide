{
  "aws-properties-cloudfront-distribution-customoriginconfig-properties": {
    "HTTPPort": {
      "Required": "No",
      "Type": "Integer",
      "UpdateRequires": "No interruption"
    },
    "HTTPSPort": {
      "Required": "No",
      "Type": "Integer",
      "UpdateRequires": "No interruption"
    },
    "OriginKeepaliveTimeout": {
      "Required": "No",
      "Type": "Integer",
      "UpdateRequires": "No interruption"
    },
    "OriginProtocolPolicy": {
      "Required": "Yes",
      "Type": "String",
      "AllowedValues": [
        "http-only",
        "https-only",
        "match-viewer"
      ],
      "UpdateRequires": "No interruption"
    },
    "OriginReadTimeout": {
      "Required": "No",
      "Type": "Integer",
      "UpdateRequires": "No interruption"
    },
    "OriginSSLProtocols": {
      "Required": "No",
      "Type": "List of String",
      "UpdateRequires": "No interruption"
    }
  }
}
