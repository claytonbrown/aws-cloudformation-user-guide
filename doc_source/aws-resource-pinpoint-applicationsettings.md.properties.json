{
  "pinpoint-applicationsettings-properties": {
    "ApplicationId": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "CampaignHook": {
      "Required": "No",
      "Type": "CampaignHook",
      "UpdateRequires": "No interruption"
    },
    "CloudWatchMetricsEnabled": {
      "Required": "No",
      "AllowedValues": [
        "True",
        "False"
      ],
      "Type": "Boolean",
      "UpdateRequires": "No interruption"
    },
    "Limits": {
      "Required": "No",
      "Type": "Limits",
      "UpdateRequires": "No interruption"
    },
    "QuietTime": {
      "Required": "No",
      "Type": "QuietTime",
      "UpdateRequires": "No interruption"
    }
  }
}
