{
  "aws-properties-codebuild-project-artifacts-properties": {
    "ArtifactIdentifier": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "No interruption"
    },
    "EncryptionDisabled": {
      "Required": "No",
      "AllowedValues": [
        "True",
        "False"
      ],
      "Type": "Boolean",
      "UpdateRequires": "No interruption"
    },
    "Location": {
      "Required": "Conditional",
      "Type": "String",
      "UpdateRequires": "No interruption"
    },
    "Name": {
      "Required": "Conditional",
      "Type": "String",
      "UpdateRequires": "No interruption"
    },
    "NamespaceType": {
      "Required": "No",
      "Type": "String",
      "AllowedValues": [
        "BUILD_ID",
        "NONE"
      ],
      "UpdateRequires": "No interruption"
    },
    "OverrideArtifactName": {
      "Required": "No",
      "AllowedValues": [
        "True",
        "False"
      ],
      "Type": "Boolean",
      "UpdateRequires": "No interruption"
    },
    "Packaging": {
      "Required": "No",
      "Type": "String",
      "AllowedValues": [
        "NONE",
        "ZIP"
      ],
      "UpdateRequires": "No interruption"
    },
    "Path": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "No interruption"
    },
    "Type": {
      "Required": "Yes",
      "Type": "String",
      "AllowedValues": [
        "CODEPIPELINE",
        "NO_ARTIFACTS",
        "S3"
      ],
      "UpdateRequires": "No interruption"
    }
  }
}
