{
  "aws-properties-iam-accesskey-properties": {
    "Serial": {},
    "Status": {
      "Required": "No",
      "Type": "String",
      "AllowedValues": [
        "Active",
        "Inactive"
      ],
      "UpdateRequires": "No interruption"
    },
    "UserName": {
      "Required": "Yes",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "128",
      "Pattern": "[\\w+=,.@-]+",
      "UpdateRequires": "Replacement"
    }
  }
}
