{
  "aws-properties-iam-accesskey-properties": {
    "Status": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "Active",
        "Inactive"
      ]
    },
    "UserName": {
      "Pattern": "[\\w+=,.@-]+",
      "Required": "Yes",
      "Maximum": "128",
      "UpdateRequires": "Replacement",
      "Minimum": "1",
      "Type": "String"
    },
    "Serial": {}
  }
}
