{
  "logs-subscriptionfilter-properties": {
    "DestinationArn": {
      "Required": "Yes",
      "Type": "String",
      "Minimum": "1",
      "UpdateRequires": "Replacement"
    },
    "FilterPattern": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "LogGroupName": {
      "Required": "Yes",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "512",
      "Pattern": "[\\.\\-_/#A-Za-z0-9]+",
      "UpdateRequires": "Replacement"
    },
    "RoleArn": {
      "Required": "No",
      "Type": "String",
      "Minimum": "1",
      "UpdateRequires": "Replacement"
    }
  }
}
