{
  "aws-properties-lambda-function-code-properties": {
    "S3Bucket": {
      "Required": "Conditional",
      "Type": "String",
      "Minimum": "3",
      "Maximum": "63",
      "Pattern": "^[0-9A-Za-z\\.\\-_]*(?<!\\.)$",
      "UpdateRequires": "No interruption"
    },
    "S3Key": {
      "Required": "Conditional",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "1024",
      "UpdateRequires": "No interruption"
    },
    "S3ObjectVersion": {
      "Required": "Conditional",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "1024",
      "UpdateRequires": "No interruption"
    },
    "ZipFile": {
      "Required": "Conditional",
      "Type": "String",
      "UpdateRequires": "No interruption"
    }
  }
}
