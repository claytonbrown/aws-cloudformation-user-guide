{
  "applicationinsights.application.Tags": {
    "Required": "No",
    "Type": "List of [Tag",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)\n\n#"
  },
  "applicationinsights.application.AutoConfigurationEnabled": {
    "Required": "No",
    "Type": "Boolean",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)",
    "AllowedValues": [
      "True",
      "False"
    ]
  },
  "applicationinsights.application.ResourceGroupName": {
    "Pattern": "[a-zA-Z0-9\\.\\-_]*",
    "Required": "Yes",
    "Maximum": "256",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)",
    "Minimum": "1",
    "Type": "String"
  },
  "applicationinsights.application.CustomComponents": {
    "Required": "No",
    "Type": "List of [CustomComponent",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "applicationinsights.application.ComponentMonitoringSettings": {
    "Required": "No",
    "Type": "List of [ComponentMonitoringSetting",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "applicationinsights.application.CWEMonitorEnabled": {
    "Required": "No",
    "Type": "Boolean",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)",
    "AllowedValues": [
      "True",
      "False"
    ]
  },
  "applicationinsights.application.OpsItemSNSTopicArn": {
    "Pattern": "^arn:aws(-\\w+)?:[\\w\\d-]+:([\\w\\d-]*)?:[\\w\\d_-]*([:/].+)*$",
    "Required": "No",
    "Maximum": "300",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)",
    "Minimum": "20",
    "Type": "String"
  },
  "applicationinsights.application.LogPatternSets": {
    "Required": "No",
    "Type": "List of [LogPatternSet",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)"
  },
  "applicationinsights.application.OpsCenterEnabled": {
    "Required": "No",
    "Type": "Boolean",
    "UpdateRequires": "[No interruption](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-no-interrupt)",
    "AllowedValues": [
      "True",
      "False"
    ]
  }
