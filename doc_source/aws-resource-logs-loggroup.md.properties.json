{
  "logs-loggroup-properties": {
    "LogGroupName": {
      "Required": "No",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "512",
      "Pattern": "[\\.\\-_/#A-Za-z0-9]+",
      "UpdateRequires": "Replacement"
    },
    "RetentionInDays": {
      "Required": "No",
      "Type": "Integer",
      "UpdateRequires": "No interruption"
    }
  }
}
