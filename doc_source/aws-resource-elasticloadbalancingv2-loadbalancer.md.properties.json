{
  "elasticloadbalancingv2-loadbalancer-properties": {
    "IpAddressType": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "dualstack",
        "ipv4"
      ]
    },
    "Subnets": {
      "Required": "No",
      "Type": "List of String",
      "UpdateRequires": "No interruption"
    },
    "Name": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "Replacement"
    },
    "LoadBalancerAttributes": {
      "Required": "No",
      "Type": "List of [LoadBalancerAttribute",
      "Maximum": "20",
      "UpdateRequires": "No interruption"
    },
    "SubnetMappings": {
      "Required": "No",
      "Type": "List of [SubnetMapping",
      "UpdateRequires": "Replacement"
    },
    "Tags": {
      "Required": "No",
      "Type": "List of [Tag",
      "UpdateRequires": "No interruption"
    },
    "SecurityGroups": {
      "Required": "No",
      "Type": "List of String",
      "UpdateRequires": "No interruption"
    },
    "Scheme": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "Replacement",
      "AllowedValues": [
        "internal",
        "internet-facing"
      ]
    },
    "Type": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "Replacement",
      "AllowedValues": [
        "application",
        "network"
      ]
    }
  }
}
