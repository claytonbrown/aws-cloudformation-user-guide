{
  "aws-properties-budgets-budget-notification-properties": {
    "Threshold": {
      "Required": "Yes",
      "Type": "Double",
      "UpdateRequires": "No interruption"
    },
    "ComparisonOperator": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "EQUAL_TO",
        "GREATER_THAN",
        "LESS_THAN"
      ]
    },
    "ThresholdType": {
      "Required": "No",
      "Type": "String",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "ABSOLUTE_VALUE",
        "PERCENTAGE"
      ]
    },
    "NotificationType": {
      "Required": "Yes",
      "Type": "String",
      "UpdateRequires": "No interruption",
      "AllowedValues": [
        "ACTUAL",
        "FORECASTED"
      ]
    }
  }
}
