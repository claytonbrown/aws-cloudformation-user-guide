{
  "aws-properties-waf-rule-predicates-properties": {
    "DataId": {
      "Required": "Yes",
      "Type": "String",
      "Minimum": "1",
      "Maximum": "128",
      "UpdateRequires": "No interruption"
    },
    "Negated": {
      "AllowedValues": [
        "True",
        "False"
      ]
    },
    "Type": {
      "Required": "Yes",
      "Type": "String",
      "AllowedValues": [
        "ByteMatch",
        "GeoMatch",
        "IPMatch",
        "RegexMatch",
        "SizeConstraint",
        "SqlInjectionMatch",
        "XssMatch"
      ],
      "UpdateRequires": "No interruption"
    }
  }
}
