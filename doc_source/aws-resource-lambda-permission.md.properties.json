{
  "lambda.permission.FunctionName": {
    "Pattern": "(arn:(aws[a-zA-Z-]*)?:lambda:)?([a-z]{2}(-gov)?-[a-z]+-\\d{1}:)?(\\d{12}:)?(function:)?([a-zA-Z0-9-_]+)(:(\\$LATEST|[a-zA-Z0-9-_]+))?",
    "Minimum": "1",
    "Required": "Yes",
    "Maximum": "140",
    "UpdateRequires": "Replacement"
  },
  "lambda.permission.SourceAccount": {
    "Pattern": "\\d{12}",
    "Required": "No",
    "Type": "String",
    "Maximum": "12",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)"
  },
  "lambda.permission.EventSourceToken": {
    "Pattern": "[a-zA-Z0-9._\\-]+",
    "Required": "No",
    "Maximum": "256",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)",
    "Minimum": "0",
    "Type": "String"
  },
  "lambda.permission.Action": {
    "Pattern": "(lambda:[*]|lambda:[a-zA-Z]+|[*])",
    "Required": "Yes",
    "Type": "String",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)"
  },
  "lambda.permission.SourceArn": {
    "Pattern": "arn:(aws[a-zA-Z0-9-]*):([a-zA-Z0-9\\-])+:([a-z]{2}(-gov)?-[a-z]+-\\d{1})?:(\\d{12})?:(.*)",
    "Required": "No",
    "Type": "String",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)\n\n#"
  },
  "lambda.permission.Principal": {
    "Pattern": "[^\\s]+",
    "Required": "Yes",
    "Type": "String",
    "UpdateRequires": "[Replacement](https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/using-cfn-updating-stacks-update-behaviors.html#update-replacement)"
  }
}
